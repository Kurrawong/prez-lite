GITHUB_TOKEN=your_github_token_here

# Optional: Pin prez-lite version/tag (default: main)
# PREZ_LITE_VERSION=main

# Optional: GitHub editing integration
# NUXT_PUBLIC_GITHUB_CLIENT_ID=
# NUXT_PUBLIC_GITHUB_AUTH_WORKER_URL=
