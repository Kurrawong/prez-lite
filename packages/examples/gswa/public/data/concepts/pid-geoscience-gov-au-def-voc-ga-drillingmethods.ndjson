{"iri":"https://pid.geoscience.gov.au/def/voc/ga/DrillingMethods/coreSonic","type":"Concept","prefLabel":{"en":"core sonic"},"altLabel":{},"definition":{"en":"Similar to the vibrocore technique, the drill stem and barrel are vibrated vertically at frequencies between 50Hz and 180Hz such that the sampler barrel advances by slicing through poorly consolidated material."},"notation":null,"inScheme":["https://pid.geoscience.gov.au/def/voc/ga/DrillingMethods"],"topConceptOf":["https://pid.geoscience.gov.au/def/voc/ga/DrillingMethods"],"broader":[],"narrower":[],"related":[],"exactMatch":[],"closeMatch":[],"broadMatch":[],"narrowMatch":[],"relatedMatch":[],"historyNote":null,"scopeNote":null,"example":null,"changeNote":null,"editorialNote":null,"citation":[],"source":[]}
