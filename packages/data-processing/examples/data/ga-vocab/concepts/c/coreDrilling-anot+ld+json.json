      {
        "@value": "core piston",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "Uses a piston to drive the core barrel. High quality long cores.",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/corePush",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:corePush"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:ga-vocabs/collections/ns1:BoreholesSamplingMethod/items/ns1:corePush"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "core push",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A pipe that is pushed into the sediment to gain a core that is no longer than 2m long. Can be used in up to 5m of water. Core pipe has a valve on top that is used to create a vacuum  at the top of the core when pulling the core out of the sediment.  ",
        "@language": "en"
      }
    ]
  },
