  {
    "@id": "https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreSonic",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:coreSonic"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:ga-vocabs/collections/ns1:BoreholesSamplingMethod/items/ns1:coreSonic"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "core sonic",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "Similar to the vibro-core technique, the drill stem and barrel are vibrated vertically at frequencies between 50Hz and 180Hz such that the sampler barrel advances by slicing through the regolith.",
        "@language": "en"
      }
    ]
  },
