        boreholessamplingmethod:pumping,
        boreholessamplingmethod:waterGrabSampling ;
    skos:prefLabel "borehole fluid sampling"@en ;
    skos:topConceptOf ns4:BoreholesSamplingMethod ;
    prez:description "Sampling of fluids (oil, gas, or water) from a borehole by an unspecified method."@en ;
    prez:identifier "boreholessamplingmethod:boreholeFluidSampling" ;
    prez:label "borehole fluid sampling"@en ;
    prez:link "/catalogs/catalogue:ga-vocabs/collections/ns4:BoreholesSamplingMethod/items/boreholessamplingmethod:boreholeFluidSampling" ;
