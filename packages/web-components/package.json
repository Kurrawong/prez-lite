{
  "name": "@prez-lite/web-components",
  "version": "0.1.0",
  "type": "module",
  "description": "Vocabulary selection web components for Prez-Lite",
  "main": "./dist/prez-lite.js",
  "module": "./dist/prez-lite.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/prez-lite.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm build:copy",
