{
  "name": "prez-lite",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Lightweight static vocabulary publishing system",
  "pnpm": {
    "onlyBuiltDependencies": ["better-sqlite3"]
  },
  "scripts": {
    "build:data": "node scripts/build-data.js",
    "build:site": "pnpm --filter web generate",
