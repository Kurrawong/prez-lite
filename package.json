{
  "name": "prez-lite",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Lightweight static vocabulary publishing system",
  "engines": {
    "node": ">=22.5.0"
  },
  "scripts": {
    "build:data": "node scripts/build-data.js",
    "dev": "pnpm --filter @prez-lite/web dev",
    "build": "pnpm build:data && pnpm --filter @prez-lite/web generate",
    "generate": "pnpm --filter @prez-lite/web generate",
    "preview": "pnpm --filter @prez-lite/web preview"
  },
  "dependencies": {
    "n3": "^2.0.1"
  }
}
