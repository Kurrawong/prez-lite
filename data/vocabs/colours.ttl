:tertiary
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "A colour made by mixing a primary colour with a neighbouring secondary colour." ;
    skos:inScheme cs: ;
    skos:prefLabel "Tertiary colour" ;
    skos:topConceptOf cs: ;
