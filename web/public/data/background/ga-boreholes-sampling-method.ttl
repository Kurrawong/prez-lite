    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/percussionDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/chamberIncubationDark> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "chamber incubation dark"@en ;
    skos:definition "A in situ method that isolates sediment and overlying water in a dark benthic chamber. Incubation water samples are collected from the benthic chamber during the incubation. The dark chamber restricts light penetration and therefore stops the process of photosynthesis. This enables benthic respiration to be measured."@en ;
    dcterms:identifier "chamberIncubationDark"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/chamberIncubation> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/chamberIncubation> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "chamber incubation"@en ;
    skos:definition "A method that isolates sediment and overlying water in a benthic chamber. Water samples are collected from the benthic chamber during the incubation."@en ;
    dcterms:identifier "chamberIncubation"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:topConceptOf <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/chamberIncubationDark> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/chamberIncubationLight> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/chamberIncubationLight> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "chamber incubation light"@en ;
    skos:definition "A in situ method that isolates sediment and overlying water in a light benthic chamber. Incubation water samples are collected from the benthic chamber during the incubation. The  transparent walls of the chamber allow light penetration and enables benthic respiration and photosynthesis to be measured."@en ;
    dcterms:identifier "chamberIncubationLight"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/chamberIncubation> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreAir> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core air"@en ;
    skos:definition "Rock or sediment core derived from a dry, air-cooled diamond drilling method."@en ;
    dcterms:identifier "coreAir"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core drilling"@en ;
    skos:definition "Sampling of a core of earth material by an unspecified drilling method"@en ;
    dcterms:identifier "coreDrilling"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/drilling> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreAir> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreBox> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreConventional> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDiamond> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreGeoprobe> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreGravity> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreKasten> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreMackereth> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/corePiston> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/corePush> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreRov> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreSonic> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreSpike> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreVibro> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/sidewallCore> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreBox> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core box"@en ;
    skos:definition "A method that obtains a large volume or large surface area core with shallow penetration and minimum disturbance. Commonly uses a double spade system."@en ;
    dcterms:identifier "coreBox"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreCatcher> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core catcher"@en ;
    skos:definition "A method by which a specimen of sediment is collected from the end of the core in the core catcher. The core catcher has curved finger-like projections that extend down to the lowest part of the core liner. The core catcher helps keep the sediment in the core liner when the core is being removed from the surface."@en ;
    dcterms:identifier "coreCatcher"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/drilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreConventional> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core conventional"@en ;
    skos:definition "Core drilling using diamond or tungsten carbide bit using a core barrel with fibreglass inner sleeves. Used in the oil industry."@en ;
    dcterms:identifier "coreConventional"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreCutter> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core cutter"@en ;
    skos:definition "A method by which a specimen of sediment is collected from the lowest part of a coring assembly at the core cutter. The core cutter has sharp edges which cut and shape the sediment entering the core liner as the corer goes into the sediment."@en ;
    dcterms:identifier "coreCutter"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/drilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDiamond> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core diamond"@en ;
    skos:definition "Rock or sediment core drilling using a liquid-cooled, diamond or tungsten carbide bit."@en ;
    dcterms:identifier "coreDiamond"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreGeoprobe> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core geoprobe"@en ;
    skos:definition "High quality core extracted by hydraulic power. Used in rough terrain, soft sand, mud, shallow water, or tight, congested areas. Long high quality cores. "@en ;
    dcterms:identifier "coreGeoprobe"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreGravity> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core gravity"@en ;
    skos:definition "Used only where short cores are required. Core barrel is gravity forced. Near surface high quality core."@en ;
    dcterms:identifier "coreGravity"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreIncubationDark> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core incubation dark"@en ;
    skos:definition "A method whereby a sediment core is collected along with overlying water. The sediment and water are incubated under no light and controlled temperature conditions."@en ;
    dcterms:identifier "coreIncubationDark"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:topConceptOf <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreIncubationLight> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core incubation light"@en ;
    skos:definition "A method whereby a sediment core is collected along with overlying water. The sediment and water are incubated under controlled light and temperature conditions."@en ;
    dcterms:identifier "coreIncubationLight"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:topConceptOf <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreKasten> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core Kasten"@en ;
    skos:definition "Large volume, reletatively undisturbed sediment core (~3m long) from a Kasten Corer. The corer is constructed of stainless steel and is square in cross-section. A weight of several hundred kilograms on top of the corer pushes it 2-3 metres into the seabed. It is designed for sedimentological and geochemical sampling of fine-grained sediments. This type of corer obtains relatively undisturbed cores and can preserve the sediment/water interface. This type of core is useful for determining sediment accumulation rates."@en ;
    dcterms:identifier "coreKasten"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreMackereth> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core Mackereth"@en ;
    skos:definition "The Mackereth is a complex coring system utilizing compressed gas to penetrate and recover single-drive moderate (up to 12 m) cores from lakes of diverse depths.  Mackereth cores can be deployed on virutally any small craft and and can be used in water depths up to 80 m."@en ;
    dcterms:identifier "coreMackereth"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/corePiston> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core piston"@en ;
    skos:definition "Uses a piston to drive the core barrel. High quality long cores."@en ;
    dcterms:identifier "corePiston"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/corePush> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core push"@en ;
    skos:definition "A pipe that is pushed into the sediment to gain a core that is no longer than 2m long. Can be used in up to 5m of water. Core pipe has a valve on top that is used to create a vacuum  at the top of the core when pulling the core out of the sediment.  "@en ;
    dcterms:identifier "corePush"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreRov> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core ROV"@en ;
    skos:definition "A core drilled from a remotely operated vehicle"@en ;
    dcterms:identifier "coreRov"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreSonic> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core sonic"@en ;
    skos:definition "Similar to the vibro-core technique, the drill stem and barrel are vibrated vertically at frequencies between 50Hz and 180Hz such that the sampler barrel advances by slicing through the regolith."@en ;
    dcterms:identifier "coreSonic"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreSpike> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core spike"@en ;
    skos:definition "A core of surficial material taken by driving a spike into the ground."@en ;
    dcterms:identifier "coreSpike"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreVeihmeyerTube> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core Veihmeyer tube"@en ;
    skos:definition "A tube that is pushed into the ground to obtain a soil sample. The Veihmeyer tube has a larger diameter at the cutting end than in the rest of the tube."@en ;
    dcterms:identifier "coreVeihmeyerTube"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:topConceptOf <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreVibro> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "core vibro"@en ;
    skos:definition "A core tube is attached to a source of mechanical vibration (the power head) and lowered into  sediment. The vibrations provide energy for rearranging the particles within the sediment in such a way that the coretube penetrates under the static weight of the vibracoring apparatus. Samples include stiff and stony clays, soft rock and sands, and saturated sediments which can not be sampled using gravity or piston corers. Also know as VIBRA. "@en ;
    dcterms:identifier "coreVibro"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/coreDrilling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/crushing> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "crushing"@en ;
    skos:definition "Sample derived by crushing a rock or sediment"@en ;
    dcterms:identifier "crushing"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:topConceptOf <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/currentMeter> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "current meter"@en ;
    skos:definition "A mooring deployed on the seabed for a finite time during a marine survey to measure sea-levels and current velocities near to the seafloor. Moorings may contain a suite of instruments measuring other parameters."@en ;
    dcterms:identifier "currentMeter"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:topConceptOf <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/diverOperatedUnderwaterStillPhotography> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "diver-operated underwater still photograph"@en ;
    skos:definition "A photograph taken by a diver using an underwater camera."@en ;
    dcterms:identifier "diverOperatedUnderwaterStillPhotography"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/underwaterStillPhotography> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/diverOperatedUnderwaterVideo> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "diver-operated underwater video"@en ;
    skos:definition "A video taken by a diver using an underwater camera."@en ;
    dcterms:identifier "diverOperatedUnderwaterVideo"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/underwaterVideo> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredgeBenthic> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "dredge benthic"@en ;
    skos:definition "Seabed sampling taken over a small area seaward of the continental shelf. Sample of the upper few cm of rocky sea floor particularly for biota."@en ;
    dcterms:identifier "dredgeBenthic"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredge> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredge> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "dredge"@en ;
    skos:definition "Sampling of either benthic or pelagic material using a dredge method. "@en ;
    dcterms:identifier "dredge"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/marineSurveySampling> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredgeBenthic> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredgeChainBag> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredgeDiamantina> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredgePipe> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/epibenthicSled> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredgeChainBag> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "dredge chain bag"@en ;
    skos:definition "Sampling of large rocks from the sea floor using a chain bag"@en ;
    dcterms:identifier "dredgeChainBag"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredge> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredgeDiamantina> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "dredge diamantina"@en ;
    skos:definition "Used as a spot dredge to quickly ascertain sea bed geology. Samples give the seabed character unless it is deep when any mud is washed out during retrieval. In shallow water this dredge will return rock, mud, benthic samples and any thing else that is on the bottom."@en ;
    dcterms:identifier "dredgeDiamantina"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredge> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredgePipe> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "dredge pipe"@en ;
    skos:definition "Sampling of small fragments and mud from the sea floor, using a metal pipe linked to the tow cable. Total weight of the dredge is around 25kg."@en ;
    dcterms:identifier "dredgePipe"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/dredge> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/filteredDustCollector> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "filtered dust collector"@en ;
    skos:definition "Sample of fine-grained solid material that is actively collected from the atmosphere by pumping and filtration."@en ;
    dcterms:identifier "filteredDustCollector"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:topConceptOf <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/filtration> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "filtration"@en ;
    skos:definition "Separation of undissolved sediment and biological particles collected from a water column by filtration. For total dissolved solids measurements the sample is filtered through a glass fibre filter under controlled conditions.  The type of filter is dependent on the method used and the purpose of the determination."@en ;
