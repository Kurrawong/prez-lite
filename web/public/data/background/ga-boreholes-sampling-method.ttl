    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/airlift> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/pumping> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/boreholeLoggingTool> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "borehole logging tool"@en ;
    skos:definition "Observation of measurement of physical properties down a borehole by an instrument in a continuous linear format.  (e.g. a gamma log)"@en ;
    dcterms:identifier "boreholeLoggingTool"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:topConceptOf <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/boreholeProdTest> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "borehole PROD test"@en ;
    skos:definition "A borehole sampling test similar to DST test, but on a larger scale and under the production conditions after the well completion is conducted."@en ;
    dcterms:identifier "boreholeProdTest"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/boreholeFluidSampling> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/boreholePurgingUntilAnalyteStabilisation> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "borehole purging until analyte stabilisation"@en ;
    skos:definition "Continued purging until key analytes were stabilised."@en ;
    dcterms:identifier "boreholePurgingUntilAnalyteStabilisation"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/pumpingHighFlow> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/pumpingHighFlow> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "pumping - high flow"@en ;
    skos:definition "Pump extraction rate induces drawdown within the bore (i.e. lowers the water level) as a result of water being purged from the bore casing - includes samples collected from functioning windmills or other equipped bores (incl. water pumped into tanks/dams) and tapping artesian water sources"@en ;
    dcterms:identifier "pumpingHighFlow"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/waterSampling> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/pumping> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/boreholePurgingUntilAnalyteStabilisation> ;
    skos:narrower <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/purgingOfThreeCaseVolumes> .

<https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/boreholeWftTest> a skos:Concept ;
    skos:inScheme <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod> ;
    skos:prefLabel "borehole WFT test"@en ;
    skos:definition "A sample is taken from a borehole with a wireline formation tester by inserting a probe into a well wall to withdraw a small amount of formation fluid and to measure the formation pore pressure. This measurement can provide formation pressures along the well hole, thereby giving a measure of pressure with depth or along a horizontal borehole."@en ;
    dcterms:identifier "boreholeWftTest"^^<http://www.w3.org/2001/XMLSchema#token> ;
    skos:broader <https://pid.geoscience.gov.au/def/voc/ga/BoreholesSamplingMethod/boreholeFluidSampling> .

