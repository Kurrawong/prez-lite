    "http://www.w3.org/2004/02/skos/core#altLabel": [
      {
        "@value": "magnetic variation",
        "@language": "en"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@value": "The angle, measured in the horizontal plane, between magnetic north and true north. This angle varies depending on the position on the Earth's surface, and changes over time. By convention, magnetic declination is positive when magnetic north is east of true north, and negative when it is to the west. Magnetic declination is a critical parameter to report in conjunction with magnetic azimuth to fully characterize the 3D orientation of a borehole.",
        "@language": "en"
      }
