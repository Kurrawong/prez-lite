    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:borehole-purpose-wa/items/ns1:permanent-storage"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "permanent storage",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A well drilled to inject fluids (e.g. CO₂) into deep subsurface rock formations for long-term storage. The process is also referred to as geosequestration (GS) in general, or more specifically as 'carbon capture and storage' (CCS) in the case of CO₂ or 'greenhouse gas storage' (GHG). Fluids are commonly captured from an industrial (e.g. steel and cement production) or energy-related source (e.g. a power plant or natural gas processing facility).",
        "@language": "en"
      }
    ]
  },
