    "https://prez.dev/label": [
      {
        "@value": "groundwater exploration",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A borehole drilled to explore the extent and properties of an aquifer and the water quality of groundwater within a given area. Being generally deeper than production or monitoring boreholes, exploration boreholes may also provide geological information on the area under investigation, including stratigraphy, biostratigraphy, lithology and/or structure.",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-monitoring",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:groundwater-monitoring"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:borehole-purpose-wa/items/ns1:groundwater-monitoring"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "groundwater monitoring",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A borehole drilled to monitor groundwater properties such as levels, quality and production impacts, as a method of assessing changes to the quantity and quality of groundwater (e.g. for piezometer installation). Groundwater data allows for sustainable water extraction and management, and the detection and remediation of groundwater contamination, among other purposes.",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-production",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:groundwater-production"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:borehole-purpose-wa/items/ns1:groundwater-production"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "groundwater production",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A borehole drilled or altered for the purpose of groundwater abstraction, whether for domestic use, agricultural use or in support of resource infrastructure (e.g. mineral holes, petroleum wells, mines, refineries). Production boreholes are often drilled in sets, known as fields, and can be in close association with exploration and/or monitoring water bores.",
        "@language": "en"
      }
    ]
  },
