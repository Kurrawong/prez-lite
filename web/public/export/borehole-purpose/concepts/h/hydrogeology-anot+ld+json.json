      {
        "@value": "disposal (hydrogeology)",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A well in which fluids are injected rather than produced, where the primary objective is to dispose of fluid/water into an isolated formation.",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-exploration",
    "https://prez.dev/identifier": [
