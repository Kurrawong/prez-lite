    "https://prez.dev/label": [
      {
        "@value": "dewatering (hydrogeology)",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A borehole drilled to remove water, as a way to facilitate construction or other activity. It is often used as a safety measure in mining below the watertable or as a preliminary step to development in an area.",
        "@language": "en"
      }
    ]
  },
