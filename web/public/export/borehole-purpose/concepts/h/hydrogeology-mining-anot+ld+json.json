    "https://prez.dev/label": [
      {
        "@value": "dewatering (mining)",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A borehole drilled and cased to enable the removal of groundwater from a mine. Mine dewatering is critical when a mine extends below the water table and groundwater does, due to gravity, infiltrate the mine workings.",
        "@language": "en"
      }
    ]
  },
