    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:geological-sample-type/items/ns1:cavings-sample"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "cavings sample",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "Pieces of rock that came from the borehole but that were not removed directly by the action of the drillbit. Cavings can be splinters, shards, chunks, and various shapes of rock, usually spalling from shale sections that have become unstable. The shape of the caving can indicate why the rock failure occurred. The term is typically used in the plural form.",
        "@language": "en"
      }
    ]
  },
