  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/mineral-house">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Mineral House is one of several offices maintained by the Department of Energy, Mines, Industry Regulation and Safety within the Perth metropolitan area, and is the department&apos;s main administrative office. The building is located at 100 Plain St, East Perth WA 6004.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities/mineral-house-foyer"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities/mineral-house-library"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities/mineral-house-other"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Mineral House</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/mineral-house-foyer">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The foyer of Mineral House has display cabinets for sections of the GSWA&apos;s rock and mineral collections. The venue is open to the public during office hours and can be accessed via the main entrance to the building at 100 Plain St, East Perth WA 6004.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Mineral House foyer</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/mineral-house-library">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Mineral House Library houses the main collections of scientific literature (books and journals) and key historical images, documents and assets belonging to the Department of Energy, Mines, Industry Regulation and Safety. The Library is located on Level 1 of Mineral House, 100 Plain St, East Perth WA 6004.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Mineral House Library</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/mineral-house-other">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Collections belonging to the Geological Survey of Western Australia are housed on various floors within Mineral House, including the ground floor (foyer), and levels 1, 4 and 5. Mineral House is located at 100 Plain St, East Perth WA 6004.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Mineral House other</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/museum-of-the-goldfields">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Museum of the Goldfields is a regional branch of the Western Australian Museum, focused on the geology, biology and culture of the Goldfields region. The building is located at 17 Hannan Street, Kalgoorlie WA 6430.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Museum of the Goldfields</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Kalgoorlie Museum</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/perth-mint">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Perth Mint is a State-owned, precious-metal refinery and tourist venture. The Perth Mint&apos;s main administrative office, plus historical displays and collections, are housed at 310 Hay St, East Perth WA 6004.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Perth Mint</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/wa-museum-boola-bardip">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">WA Museum Boola Bardip houses the Western Australian Museum&apos;s primary displays in Perth (including Earth and planetary sciences, terrestrial biology and cultural displays). The building is located in the Perth Cultural Centre, Perth WA 6000.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
