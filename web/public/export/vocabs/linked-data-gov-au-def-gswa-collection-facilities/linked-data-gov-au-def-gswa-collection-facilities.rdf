  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/carlisle-laboratory">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Carlisle laboratory is a Geological Survey of Western Australia facility undertaking in-house sample processing (mainly for geochronology) that also houses the Petrographic slide collection. The building is located on the Carlisle site at 37 Harris St, Carlisle WA 6101.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Carlisle laboratory</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/carlisle-records-building">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Carlisle Records building is a Department of Energy, Mines, Industry Regulation and Safety facility housing various key collections, including departmental Records (physical and digital), library assets and geological slides. The building is located on the Carlisle site at 37 Harris St, Carlisle WA 6101.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Carlisle Records building</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/field-logistics">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The field logistics building is a Geological Survey of Western Australia facility housing field vehicles and equipment, plus physical assets relating to the petroleum relinquishment and paleontology collections. The building is located on the Carlisle site at 37 Harris St, Carlisle WA 6101.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Field logistics</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Carlisle Stores and Transport building</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/perth-core-library">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Perth Core Library is one of two Geological Survey of Western Australia storage and viewing facilities housing geological exploration materials (predominantly drillcores and samples) and related media. The building is located on the Carlisle site at 37 Harris Street, Carlisle WA 6101.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Perth Core Library</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/harry-butler-research-centre">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Harry Butler Research Centre houses the collections and research staff of the Western Australian Museum. The building is located at 49 Kew St, Welshpool WA 6106.</skos:definition>
