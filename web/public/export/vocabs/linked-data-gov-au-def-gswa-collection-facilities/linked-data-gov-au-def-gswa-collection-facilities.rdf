    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Boola Bardip</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">WA Museum</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-collection-facilities/western-australian-museum">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Western Australian Museum manages the State&apos;s primary archive of cultural and natural history assets. Established in 1891, the Western Australian Museum now comprises six public sites and a research and collections centre. The Museum is managed by the Department of Local Government, Sport and Cultural Industries.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities/harry-butler-research-centre"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities/museum-of-the-goldfields"/>
