    <skos:historyNote>Mineral resources industry concept. Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-status-wa"/>
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-status/abandoned-bogged"/>
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-status/abandoned-caving-broken-ground"/>
