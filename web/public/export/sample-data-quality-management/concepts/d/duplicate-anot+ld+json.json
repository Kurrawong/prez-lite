    "https://prez.dev/label": [
      {
        "@value": "laboratory duplicate",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A second weighing of a sample taken from the same container as the original under laboratory conditions and analysed in the same analytical batch.",
        "@language": "en"
      }
    ]
  },
  {
