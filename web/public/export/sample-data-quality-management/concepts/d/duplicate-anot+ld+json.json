  {
    "@id": "https://linked.data.gov.au/def/sample-data-quality-management/field-duplicate",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:field-duplicate"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:sample-data-quality-management/items/ns1:field-duplicate"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "field duplicate",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A second sample of equal size (mass) to the original, collected as close as possible to the location of the original sample under comparable conditions and using an identical sampling procedure. Field duplicates are typically collected at the same time as the original and analysed within the same analytical batch to ensure that the only difference in the results is related to the sampling variance.",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/sample-data-quality-management/laboratory-duplicate",
    "https://prez.dev/identifier": [
