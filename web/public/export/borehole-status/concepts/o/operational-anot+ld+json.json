    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:borehole-status-wa/items/ns1:drilling-completed-operational"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "drilling completed - operational",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "Drilling of the borehole is completed, but operations involving the borehole are continuing (e.g. installation of production infrastructure, wirelogging, etc.). Note that in the Australian Coal Logging Standards v3.1 (CoalLog), 'completed' status is closer in meaning to 'abandoned' in this vocabulary).",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/borehole-status-wa/in-progress",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:in-progress"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:borehole-status-wa/items/ns1:in-progress"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "in progress",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "The status of a borehole commencing from the rig being moved onsite up until the time that drilling is completed. Note that this stage does not include any operations at the drillsite and within the borehole that follow the completion of drilling the hole. Note that this corresponds to the status of 'drilling' in the PPDM Wellbore Status vocabulary 2014.",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/borehole-status-wa/injecting",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:injecting"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:borehole-status-wa/items/ns1:injecting"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "injecting",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "The status of a borehole during which fluids (commonly gas and water) are injected rather than produced, typically with the primary objective of maintaining/increasing reservoir pressure to enhance production. Injection wells are also used to dispose of treated wastewater, recharge aquifers, tap geothermal energy, and in environmental remediation for the cleanup of contaminated soil or groundwater; the concept includes injecting for permanent storage. Boreholes/wells are specifically designed/drilled for this purpose, although a production well can be converted into an injection well and vice versa.",
        "@language": "en"
      }
    ]
  },
