    ],
    "https://prez.dev/label": [
      {
        "@value": "biogeochemistry",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A highly interdisciplinary field involving the integration of biology, geology and chemistry, and encompassing the study of the chemical, physical, geological and biological processes and reactions that govern the composition of the natural environment (including the biosphere, hydrosphere, atmosphere, pedosphere and the lithosphere).",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/earthsci-for/exploration-geochemistry",
