    "https://prez.dev/label": [
      {
        "@value": "regolith chemistry",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "The study of the chemical composition of materials and components making up the surficial, weathered, and usually unconsolidated layer that nearly everywhere forms the surface of the land and overlies or covers the bedrock. Regolith includes rock debris of all kinds, volcanic ash, glacial drift, alluvium, loess and eolian deposits, vegetal accumulations, and soil.",
        "@language": "en"
      }
    ]
  },
