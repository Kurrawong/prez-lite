    "https://prez.dev/label": [
      {
        "@value": "Geographic Information Science (GIScience)",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "Geographic Information Science (GIScience) is the basic research field that seeks to redefine geographic concepts and their use in the context of geographic information systems. GIScience also examines the impacts of GIS on individuals and society, and the influences of society on GIS. GIScience re-examines some of the most fundamental themes in traditional spatially oriented fields such as geography, cartography, and geodesy, while incorporating more recent developments in cognitive and information science",
        "@language": "en"
      }
    ]
