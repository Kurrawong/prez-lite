      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "metamorphic petrology",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "The branch of petrology that describes the composition, characteristics, textures, occurrence and origin of metamorphic rocks, i.e. those derived from the transformation of existing rocks into new types. It relies heavily on the use of chemistry, chemical methods, and phase diagrams.",
        "@language": "en"
      }
    ]
  },
