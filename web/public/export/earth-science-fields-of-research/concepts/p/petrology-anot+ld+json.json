      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "igneous petrology",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "The branch of petrology that describes the composition, characteristics, textures, occurrence and origin of igneous rocks, i.e. those that are formed from magma. It relies heavily on the use of chemistry, chemical methods, and phase diagrams.",
        "@language": "en"
      }
    ]
  },
  {
