    "https://prez.dev/label": [
      {
        "@value": "ore petrology",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "The branch of petrology that describes the composition, characteristics, textures, occurrence and origin of ore minerals, i.e. of naturally occurring minerals that contain valuable elements or minerals in sufficient quantities to be economically mined and processed for their desired metal or mineral content.",
        "@language": "en"
      }
    ]
  },
