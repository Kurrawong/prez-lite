  {
    "@id": "https://linked.data.gov.au/def/earthsci-for/geometallurgy",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:geometallurgy"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:earthsci-for/items/ns1:geometallurgy"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "geometallurgy",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "The practice of combining geology or geostatistics with extractive metallurgy, to create a spatially or geologically based predictive model to optimize the efficiency of mineral processing plants. Geometallurgy specifically maps the 3D variability in petrophysical and mineralogical properties of an entire orebody, to define domains that require different mining and metallurgical processing techniques, to maximize recovery of the saleable product at minimum cost.",
        "@language": "en"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/earthsci-for/grade-control",
    "https://prez.dev/identifier": [
      {
        "@value": "ns1:grade-control"
      }
    ],
    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:earthsci-for/items/ns1:grade-control"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "grade control",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "Grade control is the process aiming to maximize the value of ore mined and fed into the mill. This is commonly achieved by systematic, closely-spaced drilling to check the quality and grade of the ore while mining is underway. Drillhole data, other sampling data and geological mapping can all be used to delineate ore for mining purposes, thus ‘controlling the grade’ of feedstock to the mill.",
        "@language": "en"
      }
    ]
  },
