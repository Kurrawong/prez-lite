    "https://prez.dev/link": [
      {
        "@value": "/catalogs/catalogue:gswa-vocabs/collections/ns1:alteration-type/items/ns1:argillic-alteration-intermediate"
      }
    ],
    "https://prez.dev/label": [
      {
        "@value": "argillic alteration - intermediate",
        "@language": "en"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A type of argilllic alteration defined by the presence of montmorillonite, illite, chlorite, kaolin group clays (kaolinite, dickite, halloysite, allophane) and minor sericite. K-feldspar may remain unaltered, and K, Ca, Mg and Na may not entirely leach out. Precursor textures are usually preserved.",
        "@language": "en"
      }
    ]
