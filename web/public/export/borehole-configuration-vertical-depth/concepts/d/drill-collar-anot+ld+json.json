    "https://prez.dev/label": [
      {
        "@value": "Drill Collar"
      }
    ],
    "https://prez.dev/description": [
      {
        "@value": "A component of a drillstring that provides weight on bit for drilling. Drill collars are thick-walled tubular pieces machined from solid bars of steel or alloy. Source: Schlumberger Oilfield Glossary"
      }
    ]
  },
  {
    "@id": "https://linked.data.gov.au/def/vertical-depth-reference-systems",
    "https://prez.dev/identifier": [
