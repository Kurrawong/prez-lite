    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/water-injection-well-fluid-energy"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/geotechnical">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geotechnical or engineering drilling is carried out to determine soil and rock characteristics in order to facilitate infrastructure and mine planning. It includes the collection of rock and soil samples to determine physical properties (e.g. strength, hardness, fracture and fault frequency) for geotechnical analysis and to identify where unsafe rock or soil conditions could trigger a structural collapse or cause a hazardous scenario during, for example, an earthquake or flooding event. Geotechnical drilling is conducted to assess the potential construction sites of both above and below ground projects, including but not limited to buildings and storage structures, dams, bridges, roads, wharves and other civil works, and below surface projects such as tunnels and shafts (including all open pits and underground mining areas), access or inspection holes, or to facilitate installation of monitoring equipment (e.g. tiltmeters/inclinometers, piezometers and other sensors). Geotechnical work can overlap with environmental drilling on sites where there is a potential for environmental hazards.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">geotechnical</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">engineering drilling</skos:altLabel>
    <skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/geotechnicalSurvey"/>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/geotech"/>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/geotechnical-properties"/>
    <skos:closeMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/geothermal"/>
    <skos:historyNote>Modified from Australian Drilling Industry Association (2020; The Drilling Manual, Sixth Edition).</skos:historyNote>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/grade-control">
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/mineral-ore-body-assessment"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/mining-development-and-production"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to estimate grade and location of resources/material in the ground (in-situ) to an acceptable degree of confidence for mining to be economic.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">grade control</skos:prefLabel>
    <skos:historyNote>Australian Drilling Industry Association (2020; The Drilling Manual, Sixth Edition).</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-exploration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to explore the extent and properties of an aquifer and the water quality of groundwater within a given area. Being generally deeper than production or monitoring boreholes, exploration boreholes may also provide geological information on the area under investigation, including stratigraphy, biostratigraphy, lithology and/or structure.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">groundwater exploration</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-monitoring">
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/groundwaterLevelMonitoring"/>
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/waterQualityMonitoring"/>
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/observation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to monitor groundwater properties such as levels, quality and production impacts, as a method of assessing changes to the quantity and quality of groundwater (e.g. for piezometer installation). Groundwater data allows for sustainable water extraction and management, and the detection and remediation of groundwater contamination, among other purposes.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">groundwater monitoring</skos:prefLabel>
    <skos:historyNote>Modified from https://www.wa.gov.au/service/natural-resources/water-resources/interactive-water-science-monitoring-and-data</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-production">
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/industrialWaterSupply"/>
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/irrigation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled or altered for the purpose of groundwater abstraction, whether for domestic use, agricultural use or in support of resource infrastructure (e.g. mineral holes, petroleum wells, mines, refineries). Production boreholes are often drilled in sets, known as fields, and can be in close association with exploration and/or monitoring water bores.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">groundwater production</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">water production</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">water supply</skos:altLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:relatedMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/waterSupply"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/hazard-identification-mapping">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/foundation-construction"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An exploratory borehole drilled to identify rock voids, and any other relevant changes in conditions of the earth before drilling, mining, tunnelling or construction commences.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hazard identification/mapping</skos:prefLabel>
    <skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/geotechnicalSurvey"/>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/foundation-construction">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Drilling to assess the stability and conditions of rocks in the substrate prior to foundation and construction activities. This drilling aims to differentiate between solid basement and rock detached (floating) from the basement, as well as boring large holes deep into the ground in order to insert structures (e.g. piles, piers, or caissons) that are used to build a foundation for a construction project.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">foundation/construction</skos:prefLabel>
    <skos:historyNote>Definition modified from Australian Drilling Industry Association (2020; The Drilling Manual, Sixth Edition) and https://blaze-equip.com/a-beginners-guide-to-foundation-drilling/.</skos:historyNote>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hazard-identification-mapping"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/in-situ-leaching-storage">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/transient-storage-gases"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled for the purpose of in-situ leaching (ISL) or solution mining of salt dome structures, specifically to create a cavern suitable for temporary storage of non-aqueous gases. Salt caverns are formed by pumping fresh water to dissolve the salt and removing the resulting brine via a single well, which then serves for gas injection and withdrawal. For storing non-aqueous gases such as hydrogen and carbon dioxide, the caverns are usually at depths of at least a few hundred metres.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">in-situ leaching (storage)</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">ISL</skos:altLabel>
    <skos:historyNote>https://www.epa.gov/uic/class-vi-wells-used-geologic-sequestration-carbon-dioxide#well_def,

Popescu, S, Radu, M, Dinescu, S and Vladuca, I 2021, Study of the possibilities of CO2 storage in the underground caverns of dissolution salt mines: MATEC Web of Conferences 342, doi:10.1051/matecconf/202134206006, https://www.matec-conferences.org/articles/matecconf/pdf/2021/11/matecconf_simpro21_06006.pdf,

RISC 2022, Hydrogen Storage Potential of Depleted Oil and Gas Fields in Western Australia Literature Review and Scoping Study: Geological Survey of Western Australia, Report 221, 179p., https://dmpbookshop.eruditetechnologies.com.au/product/hydrogen-storage-potential-of-depleted-oil-and-gas-fields-in-western-australia-literature-review-and-scoping-study.do</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/transient-storage-gases">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/storage"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well used to inject and subsequently recover natural gas or other gaseous material (e.g. manufactured hydrogen) into/from subsurface rock formations.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">transient storage - gases</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://iadclexicon.org/gas-storage-well-or-storage-well/</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">temporary storage</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/in-situ-leaching-storage"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/managed-aquifer-recharge">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/reinjection-hydrogeology"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled for the intentional recharge of an aquifer under controlled conditions for subsequent recovery, demonstrable environmental benefit, or mitigation of the impacts of abstraction. This recharge must be an additional contribution to an aquifer, not a return of abstracted water.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">managed aquifer recharge</skos:prefLabel>
    <skos:historyNote>Department of Water and Environmental Regulation 2021, Policy: Managed aquifer recharge in Western Australia: Department of Water and Environmental Regulation, Joondalup, 18 p., https://www.wa.gov.au/system/files/2021-01/Policy_Managed_aquifer_recharge_in_Western_Australia.pdf</skos:historyNote>
    <skos:relatedMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/recharge"/>
    <skos:relatedMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/water-injection"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/reinjection-hydrogeology">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled for the purpose of pumping water back into an aquifer.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">reinjection (hydrogeology)</skos:prefLabel>
    <skos:historyNote>Modified from Department of Water 2013, Western Australian water in mining guideline: Department of Water, Water Licensing Delivery Series Report 12, 75p., https://www.wa.gov.au/system/files/2022-10/Western-Australian-water-in-mining-guideline.pdf.</skos:historyNote>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/managed-aquifer-recharge"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/materials-characterization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/environmental"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to identify the physical and geochemical properties of materials and whether they have the potential to cause environmental harm and/or contribute to, or detract from, the success of rehabilitation and closure. This drilling focuses on identification of fibrous materials, radioactive materials, materials capable of generating acid and/or metalliferous drainage (including neutral drainage and saline drainage), and dispersive and/or erosive material.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">materials characterization</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://dmp.wa.gov.au/documents/Environment/REC-EC-115D.pdf</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">materials characterisation</skos:altLabel>
    <skos:historyNote>Definition adapted from DEMIRS, Mining Proposal Guidance v 4.0, January 2023. Section 8.3 Materials Characterisation (https://dmp.wa.gov.au/documents/Environment/REC-EC-115D.pdf).</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/metallurgical">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/mining-development-and-production"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to obtain samples for metallurgical test work, i.e. to understand the properties of metals and their production and purification.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metallurgical</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/observation-fluid-energy">
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/observation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-fluid-energy"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A service well drilled with the purpose of monitoring the movement of gases and liquids in the subsurface, formation pressure and temperature, seismicity, and injected CO2 plumes.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">observation (fluid energy)</skos:prefLabel>
    <skos:historyNote>Compiled in collaboration with the Australian Geothermal Association.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/paste-fill">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A cased borehole drilled for paste filling (or cemented back filling) in an underground mine, i.e. to backfill underground mine stopes and/or drives to facilitate removal of remnant ore material while retaining geotechnical stability.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">paste fill</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.agedevelopments.com.au/mining-geotechnical/paste-fill-borehole/</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">cemented back filling</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">paste filling</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">paste-fill</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/permanent-storage">
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-purpose/carbon-capture-and-storage"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/storage"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well drilled to inject fluids (e.g. CO) into deep subsurface rock formations for long-term storage. The process is also referred to as geosequestration (GS) in general, or more specifically as &apos;carbon capture and storage&apos; (CCS) in the case of CO or &apos;greenhouse gas storage&apos; (GHG). Fluids are commonly captured from an industrial (e.g. steel and cement production) or energy-related source (e.g. a power plant or natural gas processing facility).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">permanent storage</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.epa.gov/uic/class-vi-wells-used-geologic-sequestration-carbon-dioxide#well_def</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">geosequestration</skos:altLabel>
    <skos:relatedMatch rdf:resource="http://linked.data.gov.au/def/borehole-purpose/greenhouse-gas-storage"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/storage">
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-purpose/carbon-capture-and-storage"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole/well drilled to examine the subsurface&apos;s ability to accommodate the temporary / permanent storage of gases and liquids, and to implement such storage.</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/explorationNaturalUndergroundStorage"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">storage</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:relatedMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/sequestration"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/transient-storage-gases"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/permanent-storage"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/transient-storage-liquids"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/production-fluid-energy">
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/hydrocarbonProduction"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-fluid-energy-resources"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well/borehole drilled to extract a fluid energy resource for commercial purposes.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/petroleum-production"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">production (fluid energy)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://ppdm.org/ppdm/PPDM/resources/PPDM/Resources.aspx</schema:citation>
    <skos:historyNote>Adapted from PPDM Well Status &amp; Classification v3.</skos:historyNote>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/borehole-purpose-wa/">PR</skos:notation>
    <skos:relatedMatch rdf:resource="http://linked.data.gov.au/def/borehole-purpose/geothermal"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/production-injection-fluid-energy">
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/petroleum-injection"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-fluid-energy-resources"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well drilled to both produce and inject fluids, either cyclically or simultaneously.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">production/injection (fluid energy)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://ppdm.org/ppdm/PPDM/resources/PPDM/Resources.aspx</schema:citation>
    <skos:historyNote>Adapted from PPDM Well Status &amp; Classification v3.</skos:historyNote>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/borehole-purpose-wa/">PR/IN</skos:notation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/reinjection-mining">
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/disposal"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled for the purpose of pumping water into an aquifer for disposal at some distance from a mine as a means of dewatering.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">reinjection (mining)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.wa.gov.au/system/files/2022-10/Western-Australian-water-in-mining-guideline.pdf</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">disposal (mining)</skos:altLabel>
    <skos:historyNote>Modified from Department of Water (2013) Western Australian water in mining guideline: Department of Water, Water Licensing Delivery Series Report 12, 75 p. https://www.wa.gov.au/system/files/2022-10/Western-Australian-water-in-mining-guideline.pdf</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/relief-fluid-energy">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-fluid-energy"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">In the petroleum and geothermal industries, a relief well is drilled to intersect and remedy a geothermal well where control has been or may be lost (e.g. blowout).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">relief (fluid energy)</skos:prefLabel>
    <skos:historyNote>Compiled in collaboration with the Australian Geothermal Association.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/remediation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/environmental"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An environmental borehole drilled to facilitate the collection of water, soil or rock samples to investigate potential contamination and the requirements for environmental remediation (e.g. cleanup, removal, containment) needed to reverse or stop environmental damage, or as part of the remediation process (e.g. pumping out groundwater for treatment or disposal).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">remediation</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.boreholesolutions.co.uk/blog/build-a-sustainable-future-with-environmental-remediation/</schema:citation>
    <skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/remediation"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/rescue">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole conducted during rescue operations to provide ventilation, water or food while larger holes are drilled to extract trapped miners.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">rescue</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:relatedMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/refuge"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/resource-definition-and-conversion-to-ore-reserves">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/mining-development-and-production"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Drilling to determine the size and grade of an orebody in order to improve confidence in the Mineral Resource and Ore Reserve classification.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/resource-assessment"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">resource definition and conversion to ore reserves</skos:prefLabel>
    <skos:historyNote>Australian Drilling Industry Association (2020; The Drilling Manual, Sixth Edition).</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/shot-hole">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/geophysical-investigation"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to insert and detonate an explosive charge to generate ground vibrations. The vibrations are recorded using sensors placed at predetermined locations in the surrounding area.</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/shotHole"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">shot hole</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">seismic drilling</skos:altLabel>
    <skos:historyNote>Australian Drilling Industry Association (2020; The Drilling Manual, Sixth Edition).</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/geophysical-investigation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled for the examination of subsurface geophysical properties (e.g. gravitational, electrical and magnetic fields, etc.).</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/geophysicalSurvey"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">geophysical investigation</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/shot-hole"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/sterilization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/mining-development-and-production"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to check that there are no minerals of current and potential economic interest located below sites for proposed infrastructure.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sterilization</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sterilisation</skos:altLabel>
    <skos:historyNote>Modified from Australian Drilling Industry Association (2020; The Drilling Manual, Sixth Edition).</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/stratigraphic-geology">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/geological-investigation"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole/well drilled variously to: provide geological information in areas of poor outcrop and inadequate drillhole coverage, such as assisting the determination of the lithological intervals and stratigraphy present at a location (e.g. GSQ stratigraphic drilling); permit the collection of pristine samples for biostratigraphic, geochemical, geochronological or other studies; investigate the presence of geological setting(s) favourable for mineralization; and/or evaluate emerging technologies. The concept is similar to &apos;stratigraphic/structural/research (mineral exploration)&apos;, but is distinct in not having a primary economic focus.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">stratigraphic (geology)</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">stratigraphic drilling</skos:altLabel>
    <skos:historyNote>Compiled from multiple sources.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/geological-investigation">
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/fault-delineation"/>
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/intrusion-delineation"/>
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/investigation"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/research"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Drilling for the primary purpose of understanding the geological characteristics of the site, such as stratigraphy, structure, and physical or chemical rock properties. These boreholes are generally drilled as part of scientific/technical investigations conducted by government or research organizations.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">geological investigation</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:relatedMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/geologicalSurvey"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/stratigraphic-geology"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/stratigraphic-research-drilling-geology"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/structural-drilling-geology"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/stratigraphic-research-drilling-geology">
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/stratigraphic"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/stratigraphicInvestigation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/geological-investigation"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to determine the lithological and stratigraphic intervals present at a location and/or to evaluate emerging technologies. Typically to enhance geological understanding of an area and/or to investigate the presence of geological setting(s) favourable for mineralization. It is most commonly associated with government drilling programs.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/stratigraphic"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">stratigraphic/research (geology)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://linked.data.gov.au/def/borehole-sub-purpose/stratigraphic</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/stratigraphic-research-fluid-energy">
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/stratigraphic"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/stratigraphicInvestigation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-fluid-energy-resources"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well drilled primarily with the intention of gathering geological data or to evaluate new technologies. The information may include stratigraphy, temperature gradient, heat flow, reservoir presence and quality.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">stratigraphic/research (fluid energy)</skos:prefLabel>
    <skos:historyNote>Compiled in collaboration with the Australian Geothermal Association.</skos:historyNote>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/borehole-purpose-wa/">STR</skos:notation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/stratigraphic-structural-research-mineral-exploration">
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/stratigraphicInvestigation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-minerals-and-basic-raw-materials"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to determine the lithological intervals present at a location and their structural and/or stratigraphic relationships. Typically drilled to determine the presence, location and extension of a potential resource, and most commonly associated with early stages of the exploration process. It can be associated with trialling of new drilling or analytical technologies.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">stratigraphic/structural/research (mineral exploration)</skos:prefLabel>
    <skos:historyNote>This term was defined by GSWA</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-minerals-and-basic-raw-materials">
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/exploration"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-minerals-and-basic-raw-materials"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Drilling to search for and delineate new sources of economic minerals or basic raw (i.e. construction) materials.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">exploration (minerals and basic raw materials)</skos:prefLabel>
    <skos:historyNote>Definition modified from Australian Drilling Industry Association (2020; The Drilling Manual, Sixth Edition).</skos:historyNote>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/stratigraphic-structural-research-mineral-exploration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/structural-drilling-geology">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/geological-investigation"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole/well drilled to investigate the attitude of strata and structural/tectonic relationships of rock units at a location. Typically to enhance geological understanding of an area and/or to investigate the presence of geological setting(s) favourable for mineralization. Most commonly associated with government drilling programs or early stages of exploration. The concept is similar to &apos;stratigraphic/structural/research (mineral exploration)&apos;, but is distinct in not having a primary economic focus.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/structure"/>
    <skos:exactMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/structure"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">structural (geology)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://linked.data.gov.au/def/borehole-sub-purpose/structure</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/stygofauna-monitoring">
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/environmental"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/environmental"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole/well drilled to identify the presence of stygofauna and facilitate the monitoring of its biodiversity and overall &apos;health&apos; parameters. Stygofauna, also known as subterranean aquatic fauna, are aquatic fauna that inhabit the water filled pore spaces, voids, cracks and fissures of most rock types and groundwater systems such as aquifers or underground caves.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/stygofauna-monitoring"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">stygofauna monitoring</skos:prefLabel>
    <skos:historyNote>Humphreys, WF 2006, Groundwater fauna paper prepared for the 2006 Australian State of the Environment Committee, Department of the Environment and Heritage, Canberra.,
Humphreys, WF, 2008, Rising Down Under: Developments in subterranean biodiversity in Australia from a groundwater fauna perspective, Invertebrate Systematics, 22, 85-101.,
https://environment.des.qld.gov.au/__data/assets/pdf_file/0029/90767/biological-assessment-background-information-on-sampling-bores-for-stygofauna.pdf</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/transient-storage-liquids">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/storage"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole/well used for the injection or withdrawal of liquid or liquefied hydrocarbons into or out of an underground hydrocarbon storage facility, including salt caverns.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">transient storage - liquids other than water</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://iadclexicon.org/hydrocarbon-storage-well-or-storage-well/</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">temporary storage</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/uncemented-backfill">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A cased borehole drilled for uncemented geomaterial filling in an underground mine. Uncemented backfill drilling is a method used to backfill underground mine stopes and/or drives to facilitate removal of remnant ore material while retaining geotechnical stability. Uncemented backfills include hydraulic fills, sand fills, aggregate fills and rock fills.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">uncemented backfill</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">aggregate filling</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">ballast fill filling</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hydraulic filling</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">rock fill filling</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sand fill filling</skos:altLabel>
    <skos:historyNote>This term was defined by GSWA</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/unknown">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The specific purpose for the drilling is unknown (or was not recorded).</skos:definition>
    <skos:exactMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/unknown"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">unknown</skos:prefLabel>
    <skos:historyNote>Added by GSWA to cater for all boreholes with purpose unknown</skos:historyNote>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/ventilation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to create a ventilation shaft in an underground mine.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">ventilation</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/water-injection-well-fluid-energy">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/injection-fluid-energy"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well/borehole drilled to force water into the target reservoir, usually to enhance energy fluid recovery.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/water-flood"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">water injection well (fluid energy)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://ppdm.org/ppdm/PPDM/resources/PPDM/Resources.aspx</schema:citation>
    <skos:historyNote>Adapted from PPDM Well Status &amp; Classification v3.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/water-monitoring-mining">
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/monitoring"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A monitoring bore drilled to identify the condition of the groundwater and of the water table at and around a mining site or part thereof (e.g. around open pits, tailings dams and waste dumps). Holes are drilled at intervals and cased with slotted casing, allowing groundwater entry and thus regular checking and sample collection/analysis to determine any variations in the height of the water table, water pressure, water quality, presence of contaminants and salinity.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">water monitoring (mining)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.lederwaterdrill.com.au/monitoring-bores/</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/water-supply-exploration-mining">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well drilled to extract water from an aquifer for use in exploration and/or mining operations, e.g. to supply water for dust control.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">water supply (exploration/mining)</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">WSW</skos:altLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/water-supply-fluid-energy">
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/water-supply"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/watersupply"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-fluid-energy"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well drilled to retrieve water to support petroleum and geothermal operations.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">water supply (fluid energy)</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">WSW</skos:altLabel>
    <skos:historyNote>Compiled in collaboration with the Australian Geothermal Association.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-minerals-and-basic-raw-materials">
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/explorationExploitationNonmetallicMineralDeposits"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/exploration"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Drilling to assist in the discovery, identification and extraction of mineral resources or basic raw (i.e. construction) materials, including the assessment of their importance and the evaluation of their economic potential. Mineral resources are inclusive of brine-hosted minerals and solid fossil fuels (e.g. coal).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">exploration and exploitation of minerals and basic raw materials</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Exploration and exploitation of mineral resources</skos:altLabel>
    <skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/explorationExploitationRawMaterial"/>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/borehole-purpose-wa/">MIN</skos:notation>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:relatedMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/mineralExplorationExtraction"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/mining-development-and-production"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-minerals-and-basic-raw-materials"/>
  </rdf:Description>

