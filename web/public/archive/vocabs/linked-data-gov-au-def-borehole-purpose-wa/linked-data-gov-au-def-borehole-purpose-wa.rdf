  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/appraisal-fluid-energy">
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/appraisal"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-fluid-energy-resources"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well drilled into a potential or known fluid energy reservoir to determine the continuation of a known pool and extent of the reservoir, in order to establish the existence of a significant quantity of recoverable fluid energy resource. Note that this concept merges the appraisal and extension definitions of PPDM Well Status and Classification (v3).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">appraisal (fluid energy)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://ppdm.org/ppdm/PPDM/resources/PPDM/Resources.aspx</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">petroleum-reservoir-characterisation</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">petroleum-reservoir-characterization</skos:altLabel>
    <skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/hydrocarbonAppraisal"/>
    <skos:historyNote>Adapted from PPDM Well Status &amp; Classification v3.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-fluid-energy-resources">
