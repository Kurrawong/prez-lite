    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well/borehole drilled to determine and evaluate the fluid energy potential of a previously undeveloped area.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">exploration (fluid energy)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://ppdm.org/ppdm/PPDM/resources/PPDM/Resources.aspx</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">NFW</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">NPW</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">New Field Wildcat</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">New Pool Wildcat</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Testing</skos:altLabel>
    <skos:historyNote>Adapted from PPDM Well Status &amp; Classification v3.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/gas-injection-well-fluid-energy">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/injection-fluid-energy"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well/borehole drilled to force gas into the target reservoir, whether to enhance fluid recovery or for temporary/permanent storage.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/gas-flood"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">gas injection well (fluid energy)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://ppdm.org/ppdm/PPDM/resources/PPDM/Resources.aspx</schema:citation>
    <skos:historyNote>Adapted from PPDM Well Status &amp; Classification v3.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/injection-fluid-energy">
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/gas-flood"/>
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/water-flood"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-fluid-energy-resources"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well drilled to introduce new or return produced fluids from a development or production well back into the subsurface after energy products have been extracted.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">injection (fluid energy)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://ppdm.org/ppdm/PPDM/resources/PPDM/Resources.aspx</schema:citation>
    <skos:historyNote>Adapted from PPDM Well Status &amp; Classification v3.</skos:historyNote>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/borehole-purpose-wa/">IN</skos:notation>
