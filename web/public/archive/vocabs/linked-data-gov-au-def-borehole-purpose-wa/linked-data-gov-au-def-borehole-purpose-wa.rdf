    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://linked.data.gov.au/def/borehole-sub-purpose/blasthole</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">blast hole</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/mining-development-and-production">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
