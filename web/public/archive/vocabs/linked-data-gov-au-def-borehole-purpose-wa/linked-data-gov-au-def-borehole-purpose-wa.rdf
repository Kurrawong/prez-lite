    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">dewatering (hydrogeology)</skos:prefLabel>
    <skos:historyNote>Modified from Department of Water 2013, Western Australian water in mining guideline: Department of Water, Water Licensing Delivery Series Report 12, 75p., https://www.wa.gov.au/system/files/2022-10/Western-Australian-water-in-mining-guideline.pdf.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled and completed for the search, abstraction, monitoring and/or disposal/injection of water.</skos:definition>
    <skos:exactMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/hydrological"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hydrogeology</skos:prefLabel>
    <skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/hydrogeologicalSurvey"/>
    <skos:historyNote>National Uniform Drillers Licensing Committee 2020, Minimum Construction Requirements for Water Bores in Australia, fourth edition, 152 p., https://adia.com.au/wp-content/uploads/2020/09/Minimum-Construction-Requirements-Edition-4.pdf</skos:historyNote>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:relatedMatch rdf:resource="http://linked.data.gov.au/def/borehole-purpose/water"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/dewatering-hydrogeology"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/disposal-hydrogeology"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-exploration"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-monitoring"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/groundwater-production"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/reinjection-hydrogeology"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/dewatering-mining">
    <skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/dewatering"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled and cased to enable the removal of groundwater from a mine. Mine dewatering is critical when a mine extends below the water table and groundwater does, due to gravity, infiltrate the mine workings.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">dewatering (mining)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Mine_dewatering</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology-mining">
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/hydrological"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/mining-development-and-production"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Hydrogeology drilling in mining operations is conducted in order to monitor and sample the water table at and around a mining site, retrieve water for use (e.g. for dust control), or to remove water from a mine (dewatering).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hydrogeology (mining)</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/dewatering-mining"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/reinjection-mining"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/water-monitoring-mining"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/water-supply-exploration-mining"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/disposal-fluid-energy">
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/disposal"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-fluid-energy"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well, often a depleted production well, into which waste fluids can be injected into an isolated formation for safe disposal. Disposal wells are typically subject to regulatory requirements to avoid the contamination of freshwater aquifers.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">disposal (fluid energy)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.slb.com/en/Terms/d/disposal_well.aspx</schema:citation>
    <skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/disposal"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/service-fluid-energy">
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/service"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/service"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-exploitation-fluid-energy-resources"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well drilled to provide maintenance and monitoring during fluid energy production, which may include supply, disposal, relief, and observation.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">service (fluid energy)</skos:prefLabel>
    <skos:historyNote>Compiled in collaboration with the Australian Geothermal Association.</skos:historyNote>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/disposal-fluid-energy"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/observation-fluid-energy"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/relief-fluid-energy"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/water-supply-fluid-energy"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/disposal-hydrogeology">
    <skos:broadMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/disposal"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/hydrogeology"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A well in which fluids are injected rather than produced, where the primary objective is to dispose of fluid/water into an isolated formation.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">disposal (hydrogeology)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://linked.data.gov.au/def/borehole-sub-purpose/disposal</schema:citation>
    <skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/disposal"/>
    <skos:historyNote>Concept adapted from Geological Survey of Queensland&apos;s Borehole sub-purpose vocabularies.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/drives-and-declines">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/service-mining"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled to enable construction of underground declines, ore drives and stope access to facilitate mining.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">drives and declines</skos:prefLabel>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/electrokinetic-in-situ-leaching">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/in-situ-leaching"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled for the purpose of electrokinetic in-situ leaching (EK-ISL), an emerging mining technology that allows for the recovery of metals from low-permeability ore bodies via boreholes without excavation or permeability enhancement techniques such as fracking. Instead, it uses electrode wells to generate electric fields and induce the transport of charged chemical species that facilitate the selective dissolution, transport and recovery of targeted metals such as copper or gold.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">electrokinetic in-situ leaching</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://az659834.vo.msecnd.net/eventsairseasiaprod/production-eecw-public/d8383096848342f387793d8744c18f46</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.uwa.edu.au/projects/mining-without-digging</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">EK-ISL</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/in-situ-leaching">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/mining-development-and-production"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled for the purpose of in-situ leaching (ISL, also called in-situ recovery or ISR) or solution mining, a mining process used to recover minerals (e.g. copper and uranium) through boreholes drilled into a deposit. It works by artificially dissolving minerals occurring naturally in a solid state and pumping the pregnant solution to the surface where the minerals can be recovered.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">in-situ leaching</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/In_situ_leach</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://world-nuclear.org/information-library/nuclear-fuel-cycle/mining-of-uranium/in-situ-leach-mining-of-uranium.aspx#</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">ISL</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">ISR</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">in-situ leach</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/electrokinetic-in-situ-leaching"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/exploration-fluid-energy">
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/hydrocarbonExploration"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/exploration"/>
