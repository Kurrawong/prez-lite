  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/environmental">
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/remediation"/>
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/borehole-sub-purpose/environmental"/>
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/environmentalMonitoring"/>
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/pollutionMonitoring"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/BoreholePurpose/environmental"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Environmental drilling is performed when soil, rock or groundwater samples are required to assess an area for contamination, or when groundwater monitoring wells or remediation wells are required for the treatment of groundwater. Typically this type of drilling is conducted for a) testing and monitoring for sites such as landfills or lagoons that have a potential for pollution, b) monitoring and testing of environmentally sensitive sites such as protected lands or public water supply wellfields, c) testing to determine the source and extent of a known pollution problem, d) sampling and well construction for recovery or remediation of pollutants in groundwater, e) identification of product (oil, petrol etc.) or human artefacts (fill, trash, archeological).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">environmental</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.precisiondrillingaustralia.com.au/services/environmental-drilling</schema:citation>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/acid-leachate-testing"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/materials-characterization"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-purpose-wa/remediation"/>
