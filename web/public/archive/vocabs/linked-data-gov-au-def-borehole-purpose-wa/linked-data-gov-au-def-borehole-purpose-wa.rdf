  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-purpose-wa/dewatering-hydrogeology">
    <skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/BoreholePurposeValue/dewatering"/>
