  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/airphoto-alos-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location of points marked on hardcopy airphotos (or images thereof) determined by visual comparison with georeferenced imagery acquired by the Advanced Land Observing Satellite (ALOS; https://earth.esa.int/eogateway/missions/alos). Captured as Map Grid of Australia (MGA) 1994 coordinates. Accuracy is typically within 25 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">capture from airphoto using ALOS (MGA)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://earth.esa.int/eogateway/missions/alos</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/airphoto-landsat-tm-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location of points marked on hardcopy airphotos (or images thereof) determined by determined visual comparison with georeferenced imagery acquired by NASA&apos;s Landsat Thematic Mapper (TM ) satellites. Captured as Map Grid of Australia (MGA) 1994 coordinates. Accuracy is typically within 25 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">capture from airphoto using Landsat TM (MGA)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://landsat.gsfc.nasa.gov/thematic-mapper/</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/airphoto-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location of points marked on hardcopy airphotos (or images thereof) determined by visual comparison with georeferenced imagery. Captured as Map Grid of Australia (MGA) 1994 coordinates. Accuracy varies from 5 to 100 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">airphoto (MGA)</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">aerial photograph (MGA)</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">air photo (MGA)</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/airphoto-alos-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/airphoto-landsat-tm-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/airphoto-orthophoto-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/airphoto-spot-p-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/rectified-airphoto-mga"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/airphoto-orthophoto-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location of points marked on hardcopy airphotos (or images thereof) determined by visual comparison with georeferenced orthophotographs, i.e. aerial photos taken with a high resolution camera that have been geometrically corrected to give a uniform scale. Captured as Map Grid of Australia (MGA) 1994 coordinates. Accuracy depends on the scale of the airphoto and ranges between 25 m and 100 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">capture from airphoto using orthophotograph (MGA)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Orthophoto</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/airphoto-spot-p-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location of points marked on hardcopy airphotos (or images thereof) determined by visual comparison with georeferenced imagery acquired by Satellite Pour l&apos;Observation de la Terre (SPOT P), a commercial satellite imaging system which uses a Panchromatic sensor to capture highly detailed black and white images. Captured as Map Grid of Australia (MGA) 1994 coordinates. Accuracy is typically 25 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">capture from airphoto using SPOT P (MGA)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://earth.esa.int/eogateway/missions/spot</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/rectified-airphoto-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location of points marked on the back of hardcopy airphotos (or images thereof) determined by: 1) scanning and rectifying position marks on the back of the airphoto; 2) scanning and rectifying the front of the airphoto; 3) overlaying a mirror image of the rectified scan of the back of the airphoto onto the rectified scan of the front. Captured as Map Grid of Australia (MGA) 1994 coordinates. Accuracy depends on the scale of the airphoto and ranges between 25 m and 100 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">capture from rectified airphoto scans (MGA)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/compass">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The determination of one&apos;s position by using a compass. The method uses angular measurements (with or without additional linear measurements) to determine the positions of points or survey lines. Accuracy is variable.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">compass surveying</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.iamcivilengineer.com/2017/12/what-is-compass-surveying.html</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/compass-clinometer"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/compass-tape"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/compass-clinometer">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Used when the determination of a position is assisted by measuring the vertical inclination between a compass user and recognized objects in the distance; commonly combined with triangulation/bearing measurements. Accuracy is variable.</skos:definition>
    <skos:historyNote>Used in the mineral resources exploration and mining industry, and captured by GSWA via mineral exploration reports.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">compass clinometer</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.youtube.com/watch?v=Sd3GtgXP-_w</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">CC</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/compass-tape">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A surveying method by which a tape (or rope, chain or any long object) is laid out in lines to determine the linear measurement and the compass is used to give bearings (angular measurement), allowing the position of objects to be calculated. Accuracy is variable.</skos:definition>
    <skos:historyNote>Used in the mineral resources exploration and mining industry, and captured by GSWA via mineral exploration reports.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">compass and tape</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">CT</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">compass and chain</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/continuous-gyroscopic-survey">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A type of downhole orientation survey that uses a non-magnetic gyroscopic surveying instrument to continuously locate points along a borehole.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">continuous gyroscopic survey</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://petrowiki.spe.org/Directional_survey_instruments</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">gyro survey</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/copied-point">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Used when a primary site is duplicated in a distinct (but often dependent) dataset. This is applied, for example, when a primary site for an environmental group site is copied from a matching mining site to enable use in a dependent database; typically, a new site identity is generated at this point that complies with the new database structure.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">copied point</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/database">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location retrieved from a separate dataset.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">database</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/copied-point"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/external-database"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/textual-load"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/external-database">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location derived from a database source external to that of the organisation recording the observation. Accuracy is estimated to be within 100 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">external database (WGS/GDA)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/textual-load">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The position of a feature is loaded from another, usually older, dataset.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">textual load</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/differential-GPS">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location acquired with a Differential Global Positioning System (DGPS), i.e. a receiver instrument that utilises a network of fixed-position, ground-based reference stations that calculate the difference between their highly accurate known position and their less accurate satellite-derived position, thereby increasing the accuracy compared to a Global Positioning System that only utilises satellite stations. Accuracy varies from 15 m to a few centimetres, but it is commonly within 1 m. Captured using the World Geodetic System 1984 or, in Australia, the Geocentric Datum of Australia 1994.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Differential GPS (WGS-84/GDA-94)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Differential_GPS</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">DGPS</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Differential GPS</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Differential Global Positioning System</skos:altLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/document">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location retrieved from a figure, image (including photos) or text, either individually or within a written document.</skos:definition>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">document</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/mineral-exploration-report"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/published-report"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/unpublished-report"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/mineral-exploration-report">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location retrieved from a map, image, or text within an exploration report.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">mineral exploration report (MGA/WGS/GDA)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/published-report">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location retrieved from a map, image, or text within a published document, e.g. an article in a journal, published thesis, etc. Accuracy estimated to be within 100 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">published report (MGA/WGS/GDA)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/unpublished-report">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location retrieved from a map, image, or text within a written, unpublished document, e.g. an internal report, unpublished thesis, etc. Accuracy estimated to be within 100 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">unpublished report (MGA/WGS/GDA)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/downhole-survey">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Location of points along a borehole obtained while conducting downhole surveys.</skos:definition>
    <skos:historyNote>Used in the mineral resources exploration and mining industry, and captured by GSWA via mineral exploration reports.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">downhole orientation survey</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.geologyforinvestors.com/down-hole-geophysics-mineral-exporation/</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">down-hole survey</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/continuous-gyroscopic-survey"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/single-shot-survey"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/single-shot-survey">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A type of downhole orientation survey that uses a single shot, photographic borehole drift indicator to locate points along a borehole. This type of survey mostly applies to older boreholes, as it has been superseded by modern tools such as the gyroscope.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">single shot survey</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.mindat.org/glossary/Eastman_survey_instrument</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.rocksoliddata.com.au/articles/downhole-survey-azimuths/</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Eastman survey</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/estimate">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Used when the location of a site (e.g. drilling site) is known to be within a mineral tenement, but no precise coordinates are known. An estimated location of this type could be given the coordinates of the central point of the tenement, or coordinates are best estimated based on available data.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">estimate within tenement</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">guess</skos:altLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/ga-common-earth-model">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location based on Geoscience Australia (GA) Common Earth Model, a unified representation of Australia&apos;s geology that allows users to explore geological aspects of an area in detail. Accuracy estimated to be around 100 m.</skos:definition>
    <skos:historyNote>Geoscience Australia&apos;s Common Earth Model is a collection of data compiled from various sources that aims to give a unified overview of the geology of Australia. The data are visualised in the Geoscience Australia 3D Data Viewer software, enabling users to interactively explore and analyze the geological features and formations of different locations across Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">GA Common Earth Model (WGS-84)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://ecat.ga.gov.au/geonetwork/dashboard/api/records/a7f66a7e-6791-4a45-8611-41050120a1eb</schema:citation>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/geological-map-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location determined from published geological maps at various scales. Captured as Map Grid of Australia (MGA) 1994 coordinates.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">geological map (MGA)</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/100k-geological-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/1M-geological-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/250k-geological-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/25k-geological-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/500k-geological-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/50k-geological-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/non-standard-geological-map-mga"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/non-standard-geological-map-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location determined from a geological map that uses a different scale, projection, or set of symbols from standard geological maps. Accuracy is estimated at 100 m. Non-standard geological maps are usually produced for specific areas or at specific sizes/scales, usually to highlight the geology of a defined area or geological region, or to present specific sets of features.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">non-standard geological map (MGA)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/google-earth">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location determined from Google Earth, a virtual model of Earth which uses the World Geodetic System 1984 (WGS-84) as its coordinate system. Typically accurate to 50 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites. Google Earth is a computer program that renders a 3D representation of Earth based primarily on satellite imagery. The program maps the Earth by superimposing satellite images, aerial photography, and GIS data onto a 3D globe, allowing users to see cities and landscapes from various angles.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Google Earth (WGS-84)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Google_Earth</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.google.com.au/earth/</schema:citation>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/gps">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location obtained by using a Global Positioning System (GPS), i.e. via triangulation using at least four satellites and a receiver (such as a smartphone or GPS device). Note that the use of drones to capture GPS coordinates falls in this category.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites. The Global Positioning System (GPS) is a satellite-based radio navigation system that provides geolocation and time information to a GPS receiver anywhere on or near the Earth where there is an unobstructed line of sight to four or more GPS satellites (each satellite sends information about its position and the current time, and the receiver calculates how far away each satellite is based on how long it took for the messages to arrive; then, the receiver uses this information to triangulate the user&apos;s exact position on Earth). It was fully operational by 1993.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">GPS</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Global_Positioning_System</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">GPS coordinates</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Global Positioning System</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Navigational GPS</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/gps-wgs84-gda94"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/gps-wgs84-gda94">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location derived by a measurement of location and time via Global Positioning System (GPS) â€” it uses multilateration to connect a receiver to a network of satellites to determine the position coordinates of the receiver. The default coordinate system for GPS is the World Geodetic System 1984; in Australia the Geocentric Datum of Australia 1994 (or 2020) is also used. Modern GPS receivers can be accurate to 10 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">GPS observation (WGS-84/GDA-94)</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/gps-wgs84-gda94-legacy"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/gps-wgs84-gda94-legacy">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Used to identify the location method for legacy points whose coordinates were captured via Global Positioning System (GPS) prior to May 1, 2000. Due to improvements in the accuracy of GPS devices over time, accuracy is estimated to vary between 100 and 200â€¯m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">GPS observation (WGS-84/GDA-94) - legacy</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/landsatTM-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location captured by using georeferenced imagery acquired by NASA&apos;s Landsat Thematic Mapper (TM) satellites. Accuracy is estimated to be within 100 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites. The Thematic Mapper is an advanced sensor carried aboard NASA Landsat satellites, used to collect high-resolution imagery in the visible, near infrared and short-wave infrared spectra of light.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Landsat TM (MGA)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://landsat.gsfc.nasa.gov/thematic-mapper/</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/lidar">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location obtained using the &apos;laser imaging detection and ranging&apos; (LiDAR) method, whereby the coordinates of a point are measured from ground control points using a laser to measure the time it takes for the reflected light to return to the receiver). Typically, a Global Navigation Satellite System receiver configured over a georeferenced control point is used to link the data in with the WGS (World Geodetic System). Horizontal and vertical accuracies vary, but can be as low as 1 cm or less.</skos:definition>
    <skos:historyNote>LiDAR is a ground or airborne surveying technology that uses laser pulses to create high-resolution models of ground elevation or underground workings.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">LiDAR</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Lidar</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">LADAR</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">LIDAR</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Lidar</skos:altLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/non-standard-topographic-map-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location determined from a topographic map that uses a different scale, projection, or set of symbols from standard topographic maps. Accuracy is estimated at 100 m. Non-standard topographic maps are usually produced for specific areas or at specific sizes/scales, usually to highlight the topography of a defined area or region, or to present specific sets of features.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">non-standard topographic map (MGA)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/orthophoto-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location captured by using orthophotographs, i.e. aerial photos taken with a high-resolution camera that have been geometrically corrected to give a uniform scale. Captured as Map Grid of Australia (MGA) 1994 coordinates. Accuracy is typically within 5 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">orthophotograph (MGA)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Orthophoto</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/panoramic-image-mga"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/panoramic-image-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location of points marked on panoramic photographs using georeferenced orthophotographs, i.e. aerial photos taken with a high resolution camera that have been geometrically corrected to give a uniform scale. Typically used to position the location of specific geological features on cliff/scarp that cannot be accessed directly during mapping. Captured as Map Grid of Australia (MGA) 1994 coordinates. Accuracy is typically within 25 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">capture from panoramic image using orthophotograph (MGA)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/rtk">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">This method involves surveying of an area to correct for common errors in Global Navigation Satellite Systems (GNSS) in real time. It uses measurements of the phase of the signal&apos;s carrier wave in addition to the information content of the signal, and relies on a single reference station or interpolated virtual station to provide real-time corrections. Typically the accuracy of this method is within 1 or 2 cm, horizontally and vertically.</skos:definition>
    <skos:historyNote>Used in the mineral resources exploration and mining industry, and captured by GSWA via mineral exploration reports.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">real time kinematic</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Real-time_kinematic_positioning</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">RTK</skos:altLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/satellite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographic location captured by using satellite images (also Earth observation imagery, spaceborne photography, or simply satellite photo) are images of Earth collected by imaging satellites operated by governments and businesses around the world.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">satellite imagery</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Satellite_imagery</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/landsatTM-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/spot-p"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/spot-p">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location captured using georeferenced imagery acquired by Satellite Pour l&apos;Observation de la Terre (SPOT P, a commercial satellite imaging system which uses a Panchromatic sensor to capture highly detailed black and white images). Accuracy is estimated to be within 20 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">SPOT P (MGA)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://earth.esa.int/eogateway/missions/spot</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/surveyed-grid">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Location of points within a large area of land which has been surveyed, measured and mapped.</skos:definition>
    <skos:historyNote>Used in the mineral resources exploration and mining industry, and captured by GSWA via mineral exploration reports.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">surveyed grid</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">SGRID</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/surveyed-ground-control"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/surveyed-ground-control">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location measured from ground control points, i.e. points whose coordinates were accurately determined by surveyors. Accuracy is typically within 1 m.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">surveyed from ground control (WGS-84/GDA-94)</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.heliguy.com/blogs/posts/guide-to-using-ground-control-points</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.pix4d.com/blog/why-ground-control-points-important/</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/topographic-map-mga">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Geographical location determined from published topographic maps at various scales. Captured as Map Grid of Australia (MGA) 1994 coordinates.</skos:definition>
    <skos:historyNote>Used by GSWA to capture locations of legacy geological observations or exploration/mining/infrastructure sites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">topographic map (MGA)</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/100k-topographic-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/1M-topographic-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/250k-topographic-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/25k-topographic-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/500k-topographic-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/50k-topographic-map-mga"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/location-method/non-standard-topographic-map-mga"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/totalstation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A total station (TS) or total station theodolite (TST) is an electronic/optical instrument used for surveying and building construction. It is an electronic transit theodolite integrated with electronic distance measurement (EDM) to measure both vertical and horizontal angles and the slope distance from the instrument to a particular point, and an on-board computer to collect data and perform triangulation calculations.</skos:definition>
    <skos:historyNote>Used in the mineral resources exploration and mining industry, and captured by GSWA via mineral exploration reports.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">total station</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Total_station</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">TS</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">TST</skos:altLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/location-method/unknown">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The location method used to derive position coordinates of a given point is not known. Accuracy cannot be estimated.</skos:definition>
    <skos:historyNote>Used by GSWA to record geological observations or exploration/mining/infrastructure sites for which the location method has not been identified.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/location-method"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">unknown</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">unknown accuracy</skos:altLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/location-method"/>
  </rdf:Description>

