  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/alunitic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">acid sulfate</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">argillic - advanced</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A type of advanced argillic alteration where alunite-group minerals (alunite, natroalunite and jarosite) are the dominant phase. Typically related to the presence of abundant sulfate ions and Al-rich protoliths.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/alunitic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/alunite-alteration"/>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">alunitic alteration</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/argillic-alteration-advanced"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/argillic-alteration-advanced">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A type of argillic alteration defined by intensive acid attack and more or less complete leaching of the alkali cations with complete destruction of the feldspars and mafic silicate phases, resulting in an assemblage of dickite, kaolinite, pyrophillite, baryte, alunite and diaspore. In addition, sulfides, topaz, tourmaline and a range of amorphous clays may be present. Found in porphyry systems, in the inner zone of hydrothermal base and precious metal-bearing veins, and most typically in high-sulfur hydrothermal systems.</skos:definition>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">argillic alteration - advanced</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Argillic_alteration</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/argillic-alteration"/>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/argillic-alteration"/>
    <skos:broadMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/advanced_argillic_alteration"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/alunitic-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/argillic-alteration-intermediate">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A type of argilllic alteration defined by the presence of montmorillonite, illite, chlorite, kaolin group clays (kaolinite, dickite, halloysite, allophane) and minor sericite. K-feldspar may remain unaltered, and K, Ca, Mg and Na may not entirely leach out. Precursor textures are usually preserved.</skos:definition>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">argillic alteration - intermediate</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Argillic_alteration</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/argillic-alteration"/>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/argillic-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/argillic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">argillization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration that introduces clay minerals including kaolinite, smectite and illite as a replacement of plagioclase and amphibole due to intense H+ metasomatism and acid leaching, at temperatures between 100° and 300°C.</skos:definition>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/argillic-alteration"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/argillic_alteration"/>
    <skos:historyNote>Definition adapted from Glossary of Geology (5th edition revised, Neuendorf et al., 2011) and Wikipedia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">argillic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Argillic_alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/argillicalteration</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/argillic-alteration-advanced"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/argillic-alteration-intermediate"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/biotitic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">biotite alteration</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The formation of biotite in country rocks, usually in early stages of hydrothermal events and commonly as a replacement of ferromagnesian minerals.</skos:definition>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">biotitic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/calc-silicate-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration defined by calcium-bearing silicate minerals such as diopside and wollastonite. Calc–silicate skarn or hornfels are typical within impure limestone or dolomite strata adjacent to an intruding igneous rock.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/calcsilicate_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/calc-silicate-alteration"/>
    <skos:historyNote>Definition adapted from Wikipedia and Glossary of Geology (5th edition revised, Neuendorf et al., 2011).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">calc-silicate alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Calc-silicate_rock</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/calcsilicaterock</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/calcification">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Carbonate alteration involving the precipitation of calcite. Note the term is most commonly used to describe the replacement of organic material (generally originally hard part) by calcium carbonate (CaCO₃) during fossilization, as well as the pedological accumulation of calcium carbonate within soil horizons.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">calcification</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/calcificationpaleont</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/carbonate-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/carbonate-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonation</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration of both Ca-rich and silicate rock involving anion metasomatism with introduction of CO₂ and formation or replacement of carbonate minerals.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/carbonate_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/carbonate-alteration"/>
    <skos:historyNote>Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonate alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/carbonatization</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/calcification"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/dolomitization"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/chloritic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chlorite alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chloritization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The formation of chlorite is typical of many hydrothermal systems, particularly in peripheral propylitic alteration or as retrograde overprinting alteration, commonly as a replacement of pyroxene and amphibole minerals or biotite.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/chloritic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/chlorite-alteration"/>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chloritic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/deuteric-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The reactions between primary magmatic minerals and the water-rich solutions that separate from the same body of magma at a late stage in its cooling history (with, for example, pyroxene being replaced by amphibole).</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/deuteric_alteration"/>
    <skos:historyNote>Definition adapted from Glossary of Geology (5th edition revised, Neuendorf et al., 2011).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">deuteric alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/deuteric</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/dolomitization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration of limestones involving cation metasomatism and base metal exchange, in which Mg2+ substitutes for Ca2+, and calcite (CaCO3) becomes dolomite [CaMg(CO3)2]. Usually through action of magnesium-bearing water (seawater or percolating meteoric water).</skos:definition>
    <skos:historyNote>Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">dolomitization</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Dolomitization</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/dolomitization</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/carbonate-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/epidote-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">epidotization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The formation of epidote in country rocks by hydrothermal alteration. Also refers to the alteration of rocks in which plagioclase feldspar is albitized, whereby the anorthite molecule is freed for the formation of epidote and zoisite (a process often accompanied by chloritization).</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/epidote_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/epidote-alteration"/>
    <skos:historyNote>Definition adapted from Glossary of Geology (5th edition revised, Neuendorf et al., 2011).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">epidote alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/epidotization</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/fenitization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A form of alkali metasomatism of quartzofeldspathic country rocks in the environs of carbonatite complexes.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">fenitization</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/fenitization</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/alkali-metasomatism"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/alkali-metasomatism">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">alkali alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">alkalic alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">alkalization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Ateration that involves the transfer of alkali ions (Na and K) from a fluid phase to adjacent rocks, i.e. the interaction of alkali (K or Na)-rich fluids with country rocks to form albite or potassium feldspar.</skos:definition>
    <skos:historyNote>Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">alkali metasomatism</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://link.springer.com/chapter/10.1007/978-3-642-75671-9_9</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/fenitization"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/fluorite-and-topaz-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration involving the destabilization and destruction of feldspars and biotite to form the assemblage quartz + muscovite with varying amounts of topaz, tourmaline, fluorite oxides (cassiterite, hematite, wolframite, scheelite and sulfides of Fe, Cu, Mo, Bi), and Cu–Bi–Pb sulfosalts. Common in continental porphyry systems and with granitic intrusions within argillaceous–arenaceous rock sequences, and with Sn–W mineralization systems.</skos:definition>
    <skos:historyNote>Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">fluorite and topaz alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/greisenization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Localized high-temperature alteration involving the destabilization and destruction of feldspars and biotite to form quartz and muscovite with varying amounts of topaz, tourmaline, lepidolite, fluorite, oxides (cassiterite, hematite, wolframite, scheelite), sulfides of Fe, Cu, Mo, Bi, and Cu–Bi–Pb sulfosalts.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/greisen"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/greisenisation"/>
    <skos:historyNote>Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">greisenization</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/greisenization</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/hematitic-jasperoid">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Advanced silica–hematite alteration, examples of which are known only from the Proterozoic rocks of the Bryah, Padbury and Yerrida Basins (formerly Glengarry Basin) in Australia. These jasperoids are hard, purple to dark purple rocks composed primarily of amethyst quartz and fine disseminated hematite and some magnetite.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hematitic jasperoid</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Jasperoid</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/jasperoid-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/jasperoid-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">jasperization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A type of hematitic alteration defined by complete replacement of limestone, and occasionally other rock types, by fine-grained quartz; commonly associated with brecciation. Involves oxidation of minerals and the formation of hematite (hematitic jasperoids), in some cases with considerable amounts of pyrite (sulfidic jasperoids).</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/red_rock_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/jasperoid-alteration"/>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">jasperoid alteration</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Jasperoid</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/jasperization</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/hematitic-alteration"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/hematitic-jasperoid"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/sulfidic-jasperoid"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/hydrothermal-alteration-other">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The presence of alteration is recognized but does not fit into any other category.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hydrothermal alteration - other</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/hydrothermal-alteration-unknown">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Used to indicate that no information is available on alteration.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hydrothermal alteration - unknown</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/hydrothermal-alteration-unspecified">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">alteration type not specified</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">undefined alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">undefined alteration assemblage</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">unknown alteration</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The presence of alteration is noted, but the type of alteration is not otherwise classified or described.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/alteration_not_specified"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/undefined-alteration-assemblage"/>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hydrothermal alteration - unspecified</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/kaolinitic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">kaolinitization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The replacement or breakdown of silicate minerals to form kaolinite and other kaolinite-group minerals (e.g. dickite), typically in acid to intermediate rocks. Kaolinization is observed in the marginal argillic zone of high-sulfidation systems, as well as in near-surface, steam-heated environments, in vein-type deposits, as massive argillic alteration zones of low-sulfidation epithermal environments, and in greisen and derived pegmatites and aplites. Kaolinite can also form during supergene weathering, but dickite is restricted to hydrothermal environments.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/kaolinitic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/kaolinitic-alteration"/>
    <skos:historyNote>Dill, HG 2016, Kaolin: Soil, rock and ore: From the mineral to the magmatic, sedimentary and metamorphic environments: Earth Science reviews, v. 161, p. 16–129.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">kaolinitic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.sciencedirect.com/science/article/pii/S0012825216301568</schema:citation>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/argillic-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/listvenitization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">silica–carbonate alteration</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration involving carbonatization and variable silicification of ultramafic (mainly peridotites and serpentinites) and mafic rocks, resulting in the formation of magnesium carbonate – quartz – mica (fuchsite) rocks. Listvenites are associated with gold mineralization and have potential for carbon storage due to the fast kinetics of carbonation reactions in serpentinized peridotites.</skos:definition>
    <skos:historyNote>Definition adapted from The free dictionary website.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">listvenitization</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://encyclopedia2.thefreedictionary.com/Listvenitization</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.sciencedirect.com/science/article/abs/pii/S0375674219303668</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/phyllic-alteration-QSP">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">QSP</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">quartz–sericite–pyrite</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sericitic alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sericitization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration typified by the assemblage quartz–sericite–pyrite (QSP) typically resulting from removal of sodium, calcium, and magnesium from calc-alkalic rocks, with pervasive replacement of silicates by micaceous phyllosilicates, muting the original rock texture. It is a common style of alteration in porphyry base-metal systems around a central zone of potassic alteration. More specifically, it relates to hydrogen-ion (H+) metasomatism of silicate minerals such as K-feldspar, plagioclase and biotite.</skos:definition>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/phyllic-alteration"/>
    <skos:historyNote>Definition compiled from multiple sources.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">phyllic alteration (QSP)</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Metasomatism#Alteration_assemblages</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Phyllic_alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/phyllicalteration</schema:citation>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/argillic-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/potassic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">&quot;adularia&quot;</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">K-feldspathisation</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">K-feldspathization</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">K-silicate alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">microclinisation</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">microclinization</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">potassic metasomatism</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">High-temperature alteration resulting in the formation of K-feldspar and biotite (porphyry systems), and adularia (epithermal systems), from the replacement of plagioclase and mafic silicate minerals. It is usually accompanied by sulfides ± anhydrite ± biotite.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/potassic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/potassic-alteration"/>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">potassic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Metasomatism#Alteration_assemblages</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/potassicalteration</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/propylitic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">propylitization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration characterized by the addition of H20 and CO2, and locally S, with no appreciable H+ metasomatism. Typically results in the formation of epidote, chlorite, Mg-Fe-Ca carbonates, albite, K-feldspar and pyrite. In some systems, propylitic alteration is mineralogically zoned from inner actinolite-rich to outer epidote-rich alteration.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/propylitic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/propyllitic-alteration"/>
    <skos:historyNote>Definition adapted from:
1) Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p.
2) Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">propylitic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Metasomatism#Alteration_assemblages</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/propylitization</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/pyritic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">pyritisation</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">pyritization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration of country rocks due to the ionic replacement by iron and sulfur atoms combining to form pyrite.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/pyritic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/pyritic-alteration"/>
    <skos:historyNote>Definition adapted from Wikipedia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">pyritic alteration</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Mineral_alteration#pyritization</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Permineralization#Pyritization</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/sulfidation"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/sulfidation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sulfide alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sulfidization</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sulphidation</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sulphide alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sulphidization</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sulfide mineral alteration of country rocks, i.e. the deposition of pyrite and other sulfides (e.g. pyrrhotite, chalcopyrite, etc.) by hydrothermal processes.</skos:definition>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/sulphide-alteration"/>
    <skos:historyNote>Definition adapted from Wikipedia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sulfidation</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Sulfidation</schema:citation>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/skarn"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/pyritic-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/rodingitization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Metasomatic alteration typical of dykes and mafic rocks in contact with or proximal to serpentinized ultramafic rocks. The mafic rocks are altered by high pH, Ca2+ and OH− fluids, which are a byproduct of the serpentinization process, and become rodingites, i.e. metasomatic rocks rich in grossular–andradite garnet, calcic pyroxene, and variable amounts of vesuvianite, epidote and scapolite.</skos:definition>
    <skos:historyNote>Definition adapted from Glossary of Geology (5th edition revised, Neuendorf et al., 2011) and Wikipedia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">rodingitization</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Rodingite</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/rodingite</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/saussuritization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">saussuritisation</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration of calcium-bearing plagioclase feldspar to a characteristic assemblage of minerals called saussurite. The typical assemblage includes albite (or oligoclase), zoisite or epidote, together with variable amounts of calcite, sericite, prehnite, and calcium-aluminium silicates other than those of the epidote group. The plagioclase may be reconstituted into a more sodium-rich variety (albite), although the original form of the crystal is usually retained. Typically related to residual fluids in the late stages of magmatic crystallization or hydrothermal alteration, and especially common in mafic rocks due to their high calcium content.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/saussuritised"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/saussurite-alteration"/>
    <skos:historyNote>Definition compiled from multiple sources.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">saussuritization</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Saussurite</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/saussuritization</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.britannica.com/science/saussuritization</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/sericitic-white-mica-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sericitization</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">phyllic (QSP) alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sericitisation</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A hydrothermal or deuteric (or metamorphic) process involving the introduction of, alteration to, or replacement by &apos;sericite&apos;, i.e. the formation of fine-grained white mica (e.g. sericite, phengite or paragonite) in country rocks. See also Phyllic (QSP) alteration.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/sericitic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/sericitic-alteration"/>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sericitic / white mica alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Sericitic_alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/sericitization</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/serpentinization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">serpentinisation</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Replacement of magnesium-rich silicate minerals (e.g. olivine, pyroxenes, or amphiboles) in dunites, peridotites or other ultramafic rocks by serpentine minerals.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/serpentinisation"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/serpentinised"/>
    <skos:historyNote>Definition adapted from Glossary of Geology (5th edition revised, Neuendorf et al., 2011) and Wikipedia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">serpentinization</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Serpentinization</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/serpentinization</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/silication">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The replacement or breakdown of silicate minerals by reaction with free silica. Not to be confused with silicification.</skos:definition>
    <skos:historyNote>Definition adapted from Glossary of Geology (5th edition revised, Neuendorf et al., 2011).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">silication</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/silication</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/silicification">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">quartz alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">silica alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">silicic alteration</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Silicification or silicic alteration is hydrothermal alteration in which the quartz, opal, chalcedony, jasper, or other forms of amorphous silica content of the rock increases. Commonly caused by a net addition of silica in the altered rock, but silicification may also occur by decomposition reactions where quartz is a byproduct (desilication) or by cation leaching in silica-stable systems. Silicification involves dissolution of rock minerals and precipitation of silica, but the term can also refer to the permeation process that occurs during petrification of organic materials (e.g. to create fossil wood). Not to be confused with silication.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/silicification"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/silica-alteration-silicified"/>
    <skos:historyNote>Definition adapted from Glossary of Geology (5th edition revised, Neuendorf et al., 2011) and Wikipedia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">silicification</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Silicification</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/silicificationmeta</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/skarn-calcic">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">calcic skarn</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Generally forms replacement zones in wallrocks (exoskarn - in limestone or occasionally mafic to intermediate volcanic rocks), or within intrusions (endoskarn). Andradite and diopside occur in oxidized assemblages related to porphyry Cu systems; grossular and hedenbergite are more common in reduced skarns (Au, W, and Sn).</skos:definition>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">skarn - calcic</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/skarn"/>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/skarn"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/skarn">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">prograde skarn</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">skarn - prograde</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The alteration of pure limestone and dolomite by introduction of Si, Al, Fe and Mg. Typical skarn minerals include pyroxene, garnet, vesuvianite, wollastonite, actinolite, magnetite or hematite, epidote and scapolite.</skos:definition>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/skarn"/>
    <skos:historyNote>Einaudi, MT, Meinert, LD and Newberry, RJ 1981, Skarn deposits, in 75th anniversary volume: 1906–1980 (1st edition) edited by B Skinner: Economic Geology Publishing Company, Littleton, Colorado, USA, p. 317–391, doi:10.5382/AV75.11.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">skarn</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/skarn</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/skarn-calcic"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/skarn-magnesian"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/skarn-retrograde-stage"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/skarn-magnesian">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">magnesian skarn</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">magnesium skarn</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Metasomatic replacement of dolomitic limestone. High-temperature magnesian skarns are characterized by forsterite and diopside and low-temperature magnesian skarns contain serpentinite and talc. Both occur as retrograde minerals after forsterite and clinopyroxene.</skos:definition>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">skarn - magnesian</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/skarn"/>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/skarn"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/skarn-retrograde-stage">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">retrograde skarn</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The overprinting of earlier (prograde) skarn alteration in the form of epidote, amphibole, chlorite, and other hydrous phases. It is typically structurally controlled and more pronounced in shallow environments; but may also affect adjacent wallrock limestone.</skos:definition>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">skarn - retrograde stage</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/skarn"/>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/skarn"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/sodic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sodic–calcic alteration</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration commonly found in deeper parts of porphyry systems and defined by albite (sodic plagioclase) and actinolite. Cf. albitic alteration.</skos:definition>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/sodic-alteration"/>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sodic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/skarn"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/sulfidic-jasperoid">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Silica–sulfide alteration of dolomites resulting in hard, dense purple-black rocks with considerable pyrite content. Sulfidic jasperoids are typically found in Nevada, Australia and Iran.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sulfidic jasperoid</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Jasperoid</schema:citation>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-type/jasperoid-alteration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/talc-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">steatitisation</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">steatitization</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">talc–carbonate alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">talc–chlorite–carbonate alteration</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Talc alteration or steatitization is the introduction of, alteration to, or replacement by, talc (steatite), especially the act or process of hydrothermal alteration of ultrabasic rocks that results in the formation of a talcose rock (such as steatite, soapstone, or relatively pure concentrations of talc). The formation of talc, usually with carbonate ± chlorite ± magnetite, as a replacement/alteration phase is typical of hydrothermal mineral deposits in Archean greenstone belts.</skos:definition>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/talc-alteration"/>
    <skos:historyNote>Definition adapted from Wikipedia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">talc alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/steatitization</schema:citation>
    <skos:broadMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/skarn"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/tourmalinization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Tourmaline alteration of country rocks, i.e. a pneumatolytic modification of a pre-existing rock in which boron-rich, late-stage fluids react with the mineral assemblage of the primary rock (usually granite), eventually producing a rock consisting of tourmaline and quartz.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/uralitisation"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/tourmaline-alteration"/>
    <skos:historyNote>Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">tourmalinization</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.oxfordreference.com/display/10.1093/oi/authority.20110803105058403</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/unaltered">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">not altered</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A rock with no visible signs of alteration, in which primary minerals, fabrics (including primary pore space) and chemistry are unchanged.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/not_altered"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/alteration-form/unaltered"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/alteration-intensity/unaltered"/>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">unaltered</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/uralitization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">uralite alteration</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">uralitisation</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The development of amphibole from pyroxene. Specifically, a late-magmatic process of replacement whereby uralitic amphibole results from alteration of primary pyroxene. Also a common metamorphic process typical of mafic and ultramafic igneous rocks containing pyroxene.</skos:definition>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">uralitization</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/zeolitic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Regionally extensive alteration around epithermal and geothermal systems. Mineralogical changes from zeolite-rich to propylitic assemblages reflect increasing depth and temperature.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/zeolitic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/zeolitic-alteration"/>
    <skos:historyNote>Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals: St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">zeolitic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.academia.edu/9125625/Atlas_of_Alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/zeolitization</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-type/hematitic-alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hematitization</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">red rock alteration</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration caused by oxidizing fluids resulting in the formation of hematite and some K-feldspar, sericite, chlorite and epidote. The term &apos;red rock alteration&apos; is not recommended for use in describing alteration, as it does not qualify the mineral changes in the rock (e.g. Na or K enrichment) and may be confused with oxidation processes more typical of weathering.</skos:definition>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/alterationtype/hematitic_alteration"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/gsq-geochem/alteration/hematite-alteration"/>
    <skos:historyNote>Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">hematitic alteration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://resource.geosciml.org/classifier/cgi/alterationtype/red_rock_alteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.tandfonline.com/doi/epdf/10.1080/08120099408728146</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-type/jasperoid-alteration"/>
  </rdf:Description>

