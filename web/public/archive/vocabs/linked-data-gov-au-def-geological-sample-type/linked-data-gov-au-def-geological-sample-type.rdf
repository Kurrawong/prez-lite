    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Rock chips or fragments derived from a drillcore of any type after drilling.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
