    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">drillcore</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/core</schema:citation>
