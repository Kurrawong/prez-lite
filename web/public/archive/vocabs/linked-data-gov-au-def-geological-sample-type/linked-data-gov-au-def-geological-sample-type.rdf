    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/core-plug"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/core-cutting">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A block cut from whole, half or quartered conventional core for petrophysical analysis. Typically starting pieces are up to 20 cm in length and may be cut shorter for further sampling depending on the techniques being applied.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">COCU</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">core cutting</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/core-plug">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A plug cut from a conventional core for analysis.</skos:definition>
    <skos:historyNote>Modified after SLB Energy Glossary definition</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">COPL</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">core plug</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.slb.com/en/terms/c/core_plug</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/drillcore"/>
    <skos:closeMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/corePlug"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/crush">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A coarse crushed rock sample comprising sub-centimetre sized chips. Often produced to generate a material fine enough to be pulverized into a powder, often for the purpose of geochemical analysis.</skos:definition>
    <skos:historyNote>Modified after https://isolab.ess.washington.edu/SOPs/rock-preparation.php</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">CRSH</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">crush</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://isolab.ess.washington.edu/SOPs/rock-preparation.php</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/sample-type/crushed-rock"/>
    <skos:closeMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/crush"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/crystal-cluster">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A group of mineral crystals formed in an open space environment and exhibiting a euhedral crystal form determined by their internal crystal structure; or a specimen derived from it. A cluster of small crystals coating the walls of a cavity is called a druse. From a collecting perspective, crystal cluster can also refer to a group of crystals obtained from the removal of matrix to expose well-formed crystals.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">CRYC</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">crystal cluster</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Crystal_cluster</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/crystal"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/crystal">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The homogeneous, solid body of a chemical element, compound, or isomorphous mixture whose constituents (such as atoms, molecules, or ions) are arranged in a highly ordered microscopic structure, forming an ordered lattice that extends in all directions; this regularly repeating atomic arrangement may be outwardly expressed by plane faces. Although a crystal is characterized first by its definite internal molecular structure and second by its external form, the term is generally used in reference to a mineral specimen that is at least partially defined by its geometrical shape consisting of flat faces with specific, characteristic orientations (i.e. natural external crystalline plane faces).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">CRYL</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">crystal</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Crystal</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/crystal</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.mindat.org/glossary/crystal</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/crystal-cluster"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/crystal-fragment"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/floater-crystal"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/geode"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/single-crystal"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/crystal-fragment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Broken piece(s) of a single crystal or crystal cluster. The breakage process can be either natural (i.e. due to geological factors/events) or artificial (e.g. occurring during sampling or specimen preparation).</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">CRYG</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">crystal fragment(s)</skos:prefLabel>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/crystal"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/crystal-model">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A teaching aid used for understanding crystallography concepts and the morphology of crystals, especially used for teaching/recognizing symmetry elements in crystals.</skos:definition>
    <skos:historyNote>Definition modified from Wikipedia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">ARTC</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">crystal model</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Crystal_model</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/model"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/model">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A facsimile representation of an object, site or location; usually in three dimensions and often at a different scale.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">ARTM</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">model</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Model</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.mindat.org/glossary/model</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/artefact"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/crystal-model"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/cut-block">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A hand sample or block with at least one side cut with a rock saw, which has not been polished.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">CUTB</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">cut block</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/doublet">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A gem substitute composed of two pieces of gem material, or one of gem material and a second of glass or synthetic, fused or cemented together; e.g. a glass imitation with a thin layer of natural gemstone (e.g. opal) fused on the top.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">GMAD</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">doublet</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/doublet</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/assembled-stone"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/assembled-stone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Any gemstone consisting of two or more parts, whether natural, synthetic, imitation, or a combination thereof; e.g. a doublet or triplet.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">GMAS</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">assembled stone</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/assembledstone</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/gemstone"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">composite stone</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/doublet"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/triplet"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/drilling-fluid-sample">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample of the fluid used for drilling (mainly drillbit lubrication) and for return of ground rock material from the bottom of the drilling interval, e.g. drilling mud.</skos:definition>
    <skos:historyNote>Modified after Geoscience Australia&apos;s definition</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">DFSA</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">drilling fluid sample</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/drillingFluidSpecimen</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/fluid-sample"/>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/sample-type/drill-fluid"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">drilling mud sample</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">mud sample</skos:altLabel>
    <skos:exactMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/drillingFluidSpecimen"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/fluid-sample">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Any liquid or gas sampled during downhole sampling processes or at the surface.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">FLSA</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">fluid sample</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
