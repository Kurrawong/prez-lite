    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/palynology-residue">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Remains of sample prepared for palynological analysis but not mounted on slides. Palynology residues are generally suspended in liquids for storage, normally glycerine. Although the term &apos;residue&apos; can be applied to materials at any part of the preparation process, the term is most often applied to the final concentration which is stored.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">RSPA</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">palynology residue</skos:prefLabel>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/residue"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/percussion-sidewall-core">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A core taken from the side of a borehole by shooting hollow, retrievable, cylindrical bullets into the formation. With full recovery, cores from typical percussion tools are 2.5 cm in diameter and between 4.4 and 5.0 cm long. Due to the shattering or compression of the sample during collection, percussion sidewall cores cannot be used for qualitative measurements such as porosity and permeability.</skos:definition>
    <skos:historyNote>Modified after SLB Energy Glossary definition</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">SWPC</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">percussion sidewall core</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.slb.com/terms/s/sidewall_core</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/sidewall-core"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/sidewall-core">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A core taken from the side of the borehole, usually by a wireline tool. These samples can provide useful details of the lithology, petrology, biostratigraphy, organic geochemistry, porosity, permeability, and hydrocarbon content of the formation.</skos:definition>
    <skos:historyNote>Modified after American Geosciences Institute Glossary of Geology (5th edition) and AAPG Wiki.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">SWCR</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sidewall core</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.slb.com/en/terms/s/sidewall_core</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://wiki.aapg.org/Overview_of_routine_core_analysis#Sidewall_core_analysis</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">MSCT</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">SWC</skos:altLabel>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/sample-type/side-wall-core"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/percussion-sidewall-core"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/rotary-sidewall-core"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/petrology-offcut">
