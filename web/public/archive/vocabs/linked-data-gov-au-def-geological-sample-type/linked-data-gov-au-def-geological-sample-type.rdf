  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/dump-mount">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A mount created by pouring or &apos;dumping&apos; material such as minerals or rock pieces/fragments onto a mounting medium such as resin, i.e. there is no hand picking of grains.</skos:definition>
