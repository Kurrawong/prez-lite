  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/polished-slab">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A thick slice of rock/mineral with a smooth and shiny/glossy surface that enhances the colour, texture and general appearance of the material.</skos:definition>
    <skos:historyNote>Modified after Geoscience Australia&apos;s definition</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">POSL</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">polished slab</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/polishedSlab</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/polished-specimen"/>
    <skos:exactMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/polishedSlab"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/polished-specimen">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Natural rocks, (gem)stones or Earth materials that have undergone a meticulous process of grinding and polishing with progressively finer abrasive materials to create a smooth surface with a significant specular reflection /enhanced lustre. This method enhances their surface, bringing out the natural colours and patterns found within the specimen.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">POSP</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">polished specimen</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://stonehards.com/articles/polished-rocks-guide/</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/polished-shaped-specimen"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/polished-slab"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/tumbled-stone"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/pulp">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A rock sample that has been pulverised to a very fine powder and homogenized to be representative of a parent sample, or interval, often for the purpose of geochemical analysis.</skos:definition>
