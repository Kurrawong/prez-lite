  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/bit-sample">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Rock sample removed from the end of a drillbit when it is brought to surface, assumed to be from the total depth of the current well interval.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">BISA</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">bit sample</skos:prefLabel>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/ditch-cuttings"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/ditch-cuttings">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Rock chips or fragments retrieved during the drilling of a petroleum well, usually mixed with drilling fluid residue. Can be either &apos;unwashed&apos; when collected straight from the well, &apos;washed&apos; when rinsed to remove drilling fluid residue, and/or &apos;dried&apos; if cleaned and processed to remove moisture.</skos:definition>
    <skos:historyNote>Modified after Geoscience Australia&apos;s definition for drill chips/cuttings</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">DTCU</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">ditch cuttings</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://pid.geoscience.gov.au/def/voc/ga/sampletype/drill_chips_cuttings</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/drill-cuttings"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">DC</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/bit-sample"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/cavings-sample"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/bulk-sample">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A smaller fraction of material (e.g. rock, regolith, sediment) that is considered to sufficiently represent certain characteristics of the parent material.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">BULK</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">bulk sample</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/cabochon">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An unfaceted cut gemstone of domed or convex form. The top is smoothly polished; the back, or base, is usually flat or slightly convex, sometimes concave, and often unpolished. The girdle outline may be round, oval, square, or of any other shape.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">GMCB</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">cabochon</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/cabochon</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/gemstone"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/gemstone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Any mineral or naturally occurring substance that, when cut and polished, has sufficient beauty, rarity, and durability for use as a personal adornment or other ornament. Organic materials such as pearl are considered gem materials rather than gemstones. Because of their desirability and demand, synthetic and imitation gemstones are common replacements for natural gemstones.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">GMST</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">gemstone</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Gemstone</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/gemstone</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.mindat.org/glossary/gemstone</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.minerals.net/mineral_glossary/gemstone.aspx</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">fine gem</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">gem</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">gem stone</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">precious stone</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">semiprecious stone</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/cabochon"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/assembled-stone"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/rough"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/imitation-gemstone"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/synthetic-gemstone"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/carved-specimen">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Any Earth material shaped into a decorative form (e.g. sphere, egg, free-form, etc.) using tools to scrape away portions of that material. The technique can be applied to any material that is solid enough to hold a form even when pieces have been removed from it, and yet soft enough for portions to be scraped away with available tools.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">POCV</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carved specimen</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://dictionary.cambridge.org/dictionary/english/carve</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Carving</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/polished-shaped-specimen"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/polished-shaped-specimen">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Any Earth material that has been modified and prepared, either by incision, by rubbing or by applying a chemical treatment, usually to create a smooth and shiny/glossy surface.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">POSH</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">polished/shaped specimen</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Polishing</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/carved-specimen"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/etched-specimen"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/polished-specimen"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/cavings-sample">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Pieces of rock that came from the borehole but that were not removed directly by the action of the drillbit. Cavings can be splinters, shards, chunks, and various shapes of rock, usually spalling from shale sections that have become unstable. The shape of the caving can indicate why the rock failure occurred. The term is typically used in the plural form.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">CASA</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">cavings sample</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.slb.com/en/terms/c/cavings</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/ditch-cuttings"/>
    <skos:closeMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/cavings"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/concretion">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A hard, compact mass or aggregate of mineral matter, normally subspherical but ranging from oblate to irregular; formed by precipitation from aqueous solution about a nucleus or centre (e.g. a shell or fossil), in the pores of a sedimentary or fragmental volcanic rock, and usually of a composition widely different from that of the rock in which it is found and from which it is rather sharply separated. It ranges in size from a small pellet-like object to a great spheroidal body as much as 3 m in diameter. Most concretions were formed during diagenesis, and many shortly after sediment deposition.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">CNCR</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">concretion</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/concretion</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
  </rdf:Description>

