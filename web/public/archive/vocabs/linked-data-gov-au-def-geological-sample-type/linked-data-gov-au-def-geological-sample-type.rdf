    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/mount"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/heavy-mineral-mount"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/imitation-gemstone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Gemstones made from materials that look (or have been treated to look) like real gemstones but have different chemical compositions and properties (cf. synthetic gemstone).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">GMIM</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">imitation gemstone</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://thenaturalgem.com/en/synthetic-and-artificial-gemstones/</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.gemrockauctions.com/learn/technical-information-on-gemstones/what-are-synthetic-gemstones-imitation-and-simulants</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/gemstone"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">simulant gemstone</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/kerogen-slide">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A strew slide prepared for transmitted light microscopy, where the original sediment sample is treated to retain all non-soluble organic matter. This compares to strew slides prepared for palynostratigraphy, where samples are oxidized prior to mounting to remove extraneous organic matter such as inertinite and vitrinite in order to highlight the assemblage of palynomorphs. As a result, kerogen slides are often generated alongside palynostratigraphic strew slides.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">KERS</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">kerogen slide</skos:prefLabel>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/slide"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/slide">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A thin, rectangular piece of glass used to hold samples to be studied under a microscope</skos:definition>
    <skos:historyNote>Modified after Wikipedia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">SLID</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">slide</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wiktionary.org/wiki/microscope_slide</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/kerogen-slide"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/microfossil-grid-slide"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/microfossil-well-slide"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/serial-sectioning-slide"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/smear-slide"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/strew-slide"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/thick-section"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/thin-section"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/lapidary-rough">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Lapidary rough is raw gemstone material used for various types of cutting and shaping, to create cabochons, carvings, beads, and other decorative items. Many different types of stones can be used as lapidary rough, including agate, jasper, turquoise and quartz.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">GMRL</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">lapidary rough</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.folkmarketgems.com/blogs/gems-and-mineral-information/understanding-facet-rough-and-cabochon-grade-lapidary-rough</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.mindat.org/glossary/rough</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/rough"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">cabochon grade gemstones</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/latex-peel">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A copy of a three-dimensional form, particularly a fossil or rock surface, made in molding latex.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">ARLP</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">latex peel</skos:prefLabel>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/cast"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/cast">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An object formed by pouring metal, plaster, fiberglass, etc., in a fluid state into a mold and letting it harden. Casts are usually made of objects (natural or artefacts) that have a special significance or scientific/monetary value due to their nature, size, rarity and provenance (e.g. nugget, meteorite, fossil); casts are also made to limit the handling of original specimens or for display purposes (e.g. for heave specimens).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">ARTA</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">cast</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.sciencing.com/mold-cast-fossils-6556194/</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.dictionary.com/browse/cast</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/replica"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/latex-peel"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/plaster-cast"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/light-mineral-concentrate">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample comprising loose mineral grains with a density of less than 2.9 g/cm³ that have been concentrated relative to the parent material usually using a technique called heavy liquid separation.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">LTCN</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">light mineral concentrate</skos:prefLabel>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/concentrate"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">light mineral separate</skos:altLabel>
    <skos:relatedMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/concentrate"/>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/fraction"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/microfossil-grid-slide">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A slide prepared for microscope analysis of microfossils, where picked specimens are mounted on a numbered grid backing for identification.</skos:definition>
    <skos:historyNote>Modified after Geoscience Australia&apos;s definition</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">GMIS</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">microfossil grid slide</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/microfossilSlide</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/slide"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">gridded slide</skos:altLabel>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/microfossilSlide"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/microfossil-mount">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Separated or isolated microfossil mounted on a backing in preparation for imaging or analysis. This term is preferred when the form of the mount is unknown, and is a partial equivalent of SEM mount / SEM stub where the mounted material is a fossil prepared for SEM microscopy. Gridded slides and well slides are occasionally referred to as mounts or mounted slides but are better considered as slide types.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">MFMT</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">microfossil mount</skos:prefLabel>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/mount"/>
    <skos:narrowMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/microfossilSlide"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/mount">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample that has been prepared for viewing/analysis/storage by either securely attaching the specimen(s) to a suitable fixed point (e.g. a slide or support) and/or by using appropriate mounting compounds/media (with or without polishing) to encapsulate specimens and hold them securely in place.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">MOUN</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">mount</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://overallscience.com/preparation-of-different-types-of-mount/</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.globalspec.com/learnmore/labware_scientific_instruments/microscopes/mounting_compounds</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:narrowMatch rdf:resource="http://linked.data.gov.au/def/sample-type/polished-block"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/grain-mount"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/dump-mount"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/microfossil-mount"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/micromount"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/polished-mount"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/sem-mount"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/microfossil-residue">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Remains of sample prepared for microfossil studies, usually stored as dried sediment. Residues may be picked (i.e. depleted of specific fossils separated for analysis) or unpicked.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">RSMF</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">microfossil residue</skos:prefLabel>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/residue"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">micropaleontology residue</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/residue">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Unspecified material leftover from sample preparation or an analytical technique.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">RSSI</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">residue</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/standardReferenceMaterial</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/microfossil-residue"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/geological-sample-type/palynology-residue"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/microfossil-well-slide">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A slide prepared for microscope analysis of microfossils, where picked specimens are contained within one (or a series of) circular wells, often unfixed.</skos:definition>
    <skos:historyNote>Modified after Geoscience Australia&apos;s definition</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">MFWS</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">microfossil well slide</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/microfossilSlide</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/slide"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">well slide</skos:altLabel>
    <skos:broadMatch rdf:resource="https://pid.geoscience.gov.au/def/voc/ga/GeologySampleType/microfossilSlide"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/geological-sample-type/micromount">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Any single or group of crystals (with or without matrix) that requires optical aid to be appreciated, commonly a hand-lens or a binocular microscope with a 10–40 times magnification. A micromount is permanently mounted in some kind of box and labelled with the name of the mineral and the location from which it came. Proper mounting both preserves delicate crystals and facilitates their handling. The term is used mostly by mineral collectors and amateur geologists.</skos:definition>
    <skos:historyNote>Term from Museum Victoria listing, definition altered from cited websites.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/geological-sample-type"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/geological-sample-type">MCMT</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">micromount</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.baltimoremineralsociety.org/what-is-micromounting.html</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.mindat.org/glossary/micromount</schema:citation>
    <schema:status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/geological-sample-type/mount"/>
