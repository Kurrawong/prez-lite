  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/siliciclastic-rock-undivided">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/sedimentary-siliciclastic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sedimentary rock that is silicon-bearing (in the form of quartz and silicates) and is composed principally of broken fragments that are derived from pre-existing rocks or minerals and that have been transported some distance from their places of origin, but otherwise uncategorized. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011). GSWA usage of this term is restricted to rocks that are partially weathered or are identified using remote sensing imagery, and the use of the related code/notation is limited to GSWA&apos;s Explanatory Notes System.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">sn</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">siliciclastic rock; undivided</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/siliciclastic-schist">
