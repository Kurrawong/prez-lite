  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/foid-bearing-volcaniclastic-granule-pebble-breccia">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
