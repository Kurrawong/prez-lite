    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">foid-bearing volcaniclastic sandstone-siltstone</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/foid-bearing-volcaniclastic-sediment-matrix-breccia">
