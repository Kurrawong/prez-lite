    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/ultramafic-volcaniclastic-breccia"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/ultramafic-volcaniclastic-breccia-sandstone"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/ultramafic-volcaniclastic-conglomerate"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/ultramafic-volcaniclastic-granule-pebble-breccia"/>
