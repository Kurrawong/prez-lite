    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chloritic schist</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chromitite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
