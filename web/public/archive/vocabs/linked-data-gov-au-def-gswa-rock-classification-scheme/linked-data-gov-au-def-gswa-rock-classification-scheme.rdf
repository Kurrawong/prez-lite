    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/mixed-limestone-dolomite"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonate-rock-undivided">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/sedimentary-carbonate"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sedimentary rock consisting of more than 50% by weight of carbonate minerals, otherwise uncategorised (adapted from Glossary of geology (2011, 5th edition revised). GSWA usage of this term is restricted to rocks that are partially weathered or are identified using remote sensing imagery, and the use of the related code/notation is limited to GSWA&apos;s Explanatory Notes System.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">kn</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonate rock; undivided</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/carbonaterock</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">calcareous rock</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonate-schist">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-schist"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A schist distinguished by the abundance of carbonate mineral phases. In GSWA usage, this term is reserved for a schist for which a protolith cannot be conclusively established. See also definition of schist/schistose structure in Fettes and Desmons, 2007, p. 193).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007) and GSWA usage.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">msk</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonate schist</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonate-schist-sedimentary">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-carbonate"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed sedimentary carbonate rock displaying a schistose structure, i.e. a schist derived from a sedimentary rock consisting mostly of carbonates. See also definition of gneiss/gneissose structure in Fettes and Desmons (2007, p. 154–155).</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mks</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonate schist (sedimentary)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonate-with-lesser-siliciclastic">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/sedimentary-carbonate"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Limestone and/or dolomite interleaved with siliciclastic rocks at outcrop scale.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">kt</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonate + lesser siliciclastic</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A collective term for an igneous (intrusive, extrusive or volcaniclastic) rock in which the modal amount of primary carbonate minerals is greater than 50%, but otherwise uncategorized (section 2.3, p. 10). Adapted from Le Maitre et al. (2005, p. 68).</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/carbonatite"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/carbonatite"/>
    <skos:historyNote>Based on the IUGS classification of igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">r</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatite</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/carbonatite</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-rock-undivided">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An igneous (intrusive, extrusive or volcaniclastic) carbonate-rich rock, otherwise uncategorized. GSWA usage of this term is restricted to rocks that are partially weathered or are identified using remote sensing imagery, and the use of the related code/notation is limited to GSWA&apos;s Explanatory Notes System.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rn</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic rock; undivided</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-breccia">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A volcaniclastic rock composed mostly of angular fragments greater than 2 mm in size consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin, with more than 50% modal carbonates. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvb</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic breccia</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-breccia-sandstone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A volcaniclastic rock composed mostly of angular fragments ranging in size from sand (0.0625 – 2 mm) and above, consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin, and with more than 50% modal carbonates. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion w, and with an overall mafic composition.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvbs</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic breccia-sandstone</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-conglomerate">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A conglomerate consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin, and with more than 50% modal carbonates. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvc</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic conglomerate</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-granule-pebble-breccia">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A breccia composed mostly of angular fragments ranging in size from granule (2–4 mm) to pebble (4–32 mm) consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin, and with more than 50% modal carbonates. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvg</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic granule/pebble breccia</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-mudstone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A mudstone (grain size ranging from silt to clay (i.e. less than 0.0625 mm) consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin, and with more than 50% modal carbonates. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvsm</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic mudstone</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A volcaniclastic rock consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin and with more than 50% modal carbonates, but otherwise uncategorized. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvr</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-sandstone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sandstone (grain size 0.0625 – 2 mm) consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin, and with more than 50% modal carbonates. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvs</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic sandstone</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-sandstone-siltstone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A volcaniclastic rock composed mostly of fragments ranging in size from sand (0.0625 – 2 mm) to silt (0.0039 – 0.0625 mm) of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin, and with more than 50% modal carbonates. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvss</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic sandstone-siltstone</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/carbonatitic-volcaniclastic-sediment-matrix-breccia">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A commonly monomictic breccia with more than 50% modal carbonates composed of angular, coarse igneous fragments cemented by a fine-grained matrix that is clastic though not necessarily strictly sedimentary in origin. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">rvsb</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">carbonatitic volcaniclastic sediment-matrix breccia</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/cataclasite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-fault-rock"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A cohesive fault rock with a poorly developed or absent schistosity, characterized by generally angular porphyroclasts and lithic fragments in a finer-grained matrix of similar composition that constitutes between 50 and 90% of the rock volume. Definition adapted from Fossen (2016) and Fettes and Desmons (2007, p. 138).</skos:definition>
    <skos:historyNote>Fossen, H 2016, Structural geology (2nd edition): Cambridge University Press, Cambridge, United Kingdom, 524p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">myx</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">cataclasite</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/cataclasite</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">mesocataclasite</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chalk">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/sedimentary-other-chemical-or-biochemical"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A soft, pure, earthy, fine-textured, usually white to light grey or buff limestone of marine origin, consisting almost wholly (90–99%) of calcite, formed mainly by calcareous tests of floating microorganisms (chiefly foraminifers) and of comminuted remains of calcareous algae (such as coccoliths and rhabdoliths), set in a structureless matrix of very finely crystalline calcite. The rock is porous, somewhat friable, and only slightly coherent. It may include the remains of bottom-dwelling organisms (e.g. echinoderms and bivalves), and nodules of chert and pyrite. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011).</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/chalk"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/chalk"/>
    <skos:historyNote>Based on the Glossary of geology (5th edition, revised; Neuendorf et al. 2011).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">ch</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chalk</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/chalk</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chemical-or-biochemical-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/sedimentary-other-chemical-or-biochemical"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sedimentary rock composed primarily of material formed directly by (a) precipitation from solution or colloidal suspension (as by evaporation), (b) by the deposition of insoluble precipitates (as by mixing solutions of two soluble salts), or (c) resulting directly or indirectly from the chemical processes and activities of living organisms — otherwise uncategorised. It generally has a crystalline texture. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011).</skos:definition>
    <skos:historyNote>Based on the Glossary of geology (5th edition, revised; Neuendorf et al. 2011).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">c</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chemical or biochemical rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/biochemicalrock</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/chemicalrock</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chemical-or-biochemical-rock-undivided">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/sedimentary-other-chemical-or-biochemical"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sedimentary rock composed primarily of material formed by precipitation from solution or colloidal suspension, by the deposition of insoluble precipitates, or resulting from the chemical processes and activities of living organisms — otherwise uncategorized. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011). GSWA usage of this term is restricted to rocks that are partially weathered or are identified using remote sensing imagery, and the use of the related code/notation is limited to GSWA&apos;s Explanatory Notes System.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">cn</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chemical or biochemical rock; undivided</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/biochemicalrock</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/chemicalrock</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chert">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/sedimentary-other-chemical-or-biochemical"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A hard, extremely dense or compact, dull to semivitreous, microcrystalline or cryptocrystalline sedimentary rock, consisting dominantly of interlocking crystals of quartz less than about 30 μm in diameter; it may contain amorphous silica (opal) or cryptocrystalline quartz (chalcedony). It sometimes contains impurities such as calcite, iron oxide, and the remains of siliceous and other organisms. It has a tough, splintery to conchoidal fracture, and may be white or variously coloured grey, green, blue, pink, red, yellow, brown, and black. Chert occurs principally as nodular or concretionary segregations (chert nodules) in limestones and dolomites, and as areally extensive layered deposits (bedded chert ); it may be an original organic or inorganic precipitate or a replacement product. The term flint is essentially synonymous, although it has been used for the dark variety of chert. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011).</skos:definition>
    <skos:historyNote>Based on the Glossary of geology (5th edition, revised; Neuendorf et al. 2011).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">cc</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chert</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/chert</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">flint</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chloritic-fault-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-fault-rock"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A rock formed as a result of deformation in a fault zone and distinguished by the abundance of chlorite minerals. In GSWA usage, this term is reserved for fault rocks for which a protolith cannot be conclusively established.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">myc</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chloritic fault rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chloritic-gneiss">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-gneiss"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A gneiss distinguished by the abundance of chlorite minerals. In GSWA usage, this term is reserved for a gneiss for which a protolith cannot be conclusively established. See also definition of gneiss/gneissose structure in Fettes and Desmons (2007, p. 154–155).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007) and GSWA usage.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mnc</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chloritic gneiss</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/gneiss</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chloritic-granofels-hornfels">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-granofels-hornfels"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A granofels/hornfels distinguished by the abundance of chlorite minerals. In GSWA usage, this term is reserved for a granofels/hornfels for which a protolith cannot be conclusively established.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mec</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">chloritic granofels/hornfels</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/chloritic-schist">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-schist"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A schist distinguished by the abundance of chlorite minerals. In GSWA usage, this term is reserved for a schist for which a protolith cannot be conclusively established. See also definition of schist/schistose structure in Fettes and Desmons, 2007, p. 193).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007) and GSWA usage.</skos:historyNote>
