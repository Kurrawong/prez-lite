  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/quartz-diorite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An intrusive rock having the composition of diorite but with an appreciable amount of quartz, i.e. between 5 and 20% of the felsic minerals, and defined modally in QAPF field 10* (Fig. 2.4, p. 22). Adapted from Le Maitre et al. (2005, p. 134).</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/quartzDiorite"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/quartz_diorite"/>
    <skos:historyNote>Based on the IUGS classification of igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">gd</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">quartz diorite</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/quartzdiorite</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/quartz-gabbro">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An intrusive rock composed mainly of calcic plagioclase, clinopyroxene and quartz. Now defined modally in QAPF field 10* (Fig. 2.4, p. 22). (cf. Le Maitre et al., 2005, p. 134)</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/quartzGabbro"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/quartz_gabbro"/>
    <skos:historyNote>Based on the IUGS classification of igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">gu</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">quartz gabbro</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/quartzgabbro</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/quartz-latite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A term originally used for a volcanic rock composed of phenocrysts of quartz, plagioclase, biotite and hornblende in a glassy matrix potentially of quartz and alkali feldspar. Now commonly used for volcanic rocks composed of alkali feldspar and plagioclase in roughly equal amounts, quartz and mafic minerals. Now defined modally in QAPF field 8* (Fig. 2.4, p. 22). Definition from Le Maitre et al. (2005, p. 135). GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/quartzLatite"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/quartz_latite"/>
    <skos:historyNote>Based on the IUGS classification of igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">fi</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">quartz latite</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/quartzlatite</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/quartz-mica-schist">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-schist"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A schist distinguished by the abundance of quartz and micaceous mineral phases. In GSWA usage, this term is reserved for a schist for which a protolith cannot be conclusively established. See also definition of schist/schistose structure in Fettes and Desmons, 2007, p. 193).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007) and GSWA usage.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">msqm</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">quartz-mica schist</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/schist</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/quartz-monzodiorite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An intrusive rock consisting essentially of sodic plagioclase, alkali feldspar, quartz and mafic minerals. Now defined modally in QAPF field 9* (Fig. 2.4, p. 22). (cf. Le Maitre et al., 2005, p. 135).</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/quartzMonzodiorite"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/quartz_monzodiorite"/>
    <skos:historyNote>Based on the IUGS classification of igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">gk</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">quartz monzodiorite</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/quartzmonzodiorite</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/quartz-monzogabbro">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An intrusive rock consisting essentially of calcic plagioclase, alkali feldspar, quartz and mafic minerals. Now defined modally in QAPF field 9* (Fig. 2.4, p. 22). (cf. Le Maitre et al., 2005, p. 135).</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/quartzMonzogabbro"/>
