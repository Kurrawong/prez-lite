    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metadiorite"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metagranitic-rock"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metagranodiorite"/>
