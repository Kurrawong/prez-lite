  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granitic-gneiss">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic intrusive rock displaying a gneissose structure, i.e. a gneiss derived from an intrusive rock with one or more felsic minerals as the main component(s). See also definition of gneiss/gneissose structure in Fettes and Desmons (2007, p. 154–155).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mgn</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granitic gneiss</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granitic-granofels-hornfels">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic intrusive rock displaying a granofelsic structure, i.e. a granofels/hornfels derived from an intrusive rock with one or more felsic minerals as the main components. See also definition of granofels/granofelsic structure in Fettes and Desmons (2007, p. 156); hornfels are granofels formed by contact metamorphism (Fettes and Desmons, 2007, p. 159).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mge</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granitic granofels/hornfels</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granitic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">In GSWA usage, a generic term for an igneous intrusive rock composed chiefly of one or more felsic, light-coloured minerals in its mode and with quartz modal content between 0 and 60%, otherwise uncategorized.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">g</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granitic rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/granitepetrology</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granitic-rock-undivided">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A demonstrably felsic igneous intrusive rocks, otherwise uncategorized. GSWA usage of this term is restricted to rocks that are partially weathered or are identified using remote sensing imagery, and the use of the related code/notation is limited to GSWA&apos;s Explanatory Notes System.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of igneous rocks (Le Maitre et al., 2005) and GSWA usage.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">gn</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granitic rock; undivided</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granitic-schist">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic intrusive rock displaying a schistose structure, i.e. a schist derived from an intrusive rock with one or more felsic minerals as the main component(s). See also definition of schist/schistose structure in Fettes and Desmons, 2007, p. 193).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mgs</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granitic schist</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granodiorite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An intrusive rock consisting essentially of quartz, sodic plagioclase and lesser amounts of alkali feldspar with minor amounts of hornblende and biotite. Now defined modally in QAPF field 4 (Fig. 2.4, p. 22). (cf. Le Maitre et al., 2005, p. 86)</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/granodiorite"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/granodiorite"/>
    <skos:historyNote>Based on the IUGS classification of igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">gg</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granodiorite</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/granodiorite</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granofels-hornfels">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-granofels-hornfels"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphic rock for which a protolith cannot be conclusively established and displaying a granofelsic structure, i.e. a structure resulting from the absence of schistosity such that the mineral grains and aggregates of mineral grains are equant (for example quartz, feldspar, garnet and pyroxene) or, if inequant, have a random orientation. Mineralogical or lithological layering may be present. Definition adapted from Fettes and Desmons (2007, p. 156). Hornfels are granofels that are typically formed by contact metasomatism and occur mostly (though not exclusively) in the innermost part of contact aureoles. Definition adapted from Fettes and Desmons (2007, p. 159) and Glossary of geology (5th edition revised; Neuendorf et al., 2011).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">me</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granofels/hornfels</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/granofels</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/hornfels</schema:citation>
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/granofels"/>
    <skos:narrowMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/hornfels"/>
    <skos:narrowMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/granofels"/>
    <skos:narrowMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/hornfels"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granofelsic-granulite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-protolith-unknown-granofels-hornfels"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A high-grade metamorphic rock, with a granoblastic texture and little foliation/lineation and in which Fe–Mg silicates are dominantly hydroxyl-free, feldspar is a critical phase, and primary muscovite is absent (cf. Fettes and Desmons, 2007, p. 156). In GSWA usage, this term is reserved for a granofels/hornfels for which a protolith cannot be conclusively established.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">meo</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granofelsic granulite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granofelsic-hornfelsic-hydrothermal-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphosed-hydrothermal"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed hydrothermal rock displaying a granofelsic structure, i.e. a granofels/hornfels derived from a hydrothermal protolith. See also definition of granofels/granofelsic structure in Fettes and Desmons (2007, p. 156); hornfels are granofels formed by contact metamorphism (Fettes and Desmons, 2007, p. 159).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007) and GSWA usage.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mze</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granofelsic/hornfelsic hydrothermal rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/hydrothermalalteration</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/granofels</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/Hornfels</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/hornfels;</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granofelsic-hornfelsic-marble">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/marble"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed sedimentary carbonate rock consisting of more than 50% volume of carbonate minerals (calcite and/or aragonite and/or dolomite) and displaying a granofelsic/hornfelsic structure. See also definition of granofels/granofelsic structure in Fettes and Desmons (2007, p. 156); hornfels are granofels formed by contact metamorphism (Fettes and Desmons, 2007, p. 159).</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mkke</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granofelsic/hornfelsic marble</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granofelsic-hornfelsic-meta-iron-formation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-other-chemical-meta-iron-formation"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed iron formation displaying a granofelsic structure, i.e. a granofels/hornfels derived from a chemical or biochemical sedimentary protolith containing at least 15% iron of sedimentary origin. See also definition of granofels/granofelsic structure in Fettes and Desmons (2007, p. 156); hornfels are granofels formed by contact metamorphism (Fettes and Desmons, 2007, p. 159).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007) and GSWA usage.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mie</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granofelsic/hornfelsic meta-iron formation</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/granofels</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/hornfels</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">itabirite</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">taconite</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granofelsic-hornfelsic-metachert">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-other-chemical"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed chert displaying a granofelsic structure, i.e. a granofels/hornfels derived from a chemical or biochemical sedimentary rock largely consisting of silica and/or quartz. See also definition of granofels/granofelsic structure in Fettes and Desmons (2007, p. 156); hornfels are granofels formed by contact metamorphism (Fettes and Desmons, 2007, p. 159).</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mce</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granofelsic/hornfelsic metachert</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granofelsic-hornfelsic-para-amphibolite-carbonate">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/para-amphibolite-derived-from-carbonate-rock"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed amphibole-rich rock derived from a sedimentary carbonate protolith and displaying a granofelsic/hornfelsic structure. See also definition of granofels/granofelsic structure in Fettes and Desmons (2007, p. 156); hornfels are granofels formed by contact metamorphism (Fettes and Desmons, 2007, p. 159).</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mkae</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granofelsic/hornfelsic para-amphibolite (carbonate)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granofelsic-hornfelsic-para-amphibolite-siliciclastic">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/para-amphibolite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed amphibole-rich rock derived from a sedimentary siliciclastic protolith and displaying a granofelsic structure. See also definition of granofels/granofelsic structure in Fettes and Desmons (2007, p. 156); hornfels are granofels formed by contact metamorphism (Fettes and Desmons, 2007, p. 159).</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mdae</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granofelsic/hornfelsic para-amphibolite (siliciclastic)</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granophyre">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A porphyritic rock of granite composition in which the groundmass alkali feldspar and quartz are in micrographic intergrowth (Le Maitre et al., 2005, p. 86). A strict and limited usage of the term is encouraged in the GSWA scheme.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of igneous rocks (Le Maitre et al., 2005) and GSWA usage.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">gv</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granophyre</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/granophyre</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/granule-conglomerate">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/conglomerate"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A consolidated rock consisting mainly of granules that are larger than a very coarse sand grain and smaller than a pebble (2–4 mm or -1 to -2 phi units) being somewhat rounded or otherwise modified by abrasion in the course of transport; a consolidated granule gravel. The term very fine pebble conglomerate has been used as a synonym. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011).</skos:definition>
    <skos:historyNote>Wentworth, CK 1922, A scale of grade and class terms for clastic sediments: The Journal of Geology, v. 30, no. 5, p. 377–392, doi:10.1086/622910.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">su</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granule conglomerate</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/granulesed</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">granulestone</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">gritstone</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">very fine pebble conglomerate</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/graphitic-fault-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
