    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/hydrothermal"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A rock comprising baryte of pegmatitic or hydrothermal origin, as vein-filling material.</skos:definition>
