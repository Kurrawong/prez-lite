    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/ultramafic-granofels-hornfels-volcanic-or-undivided"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/ultramafic-schist-volcanic-or-undivided"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-ultramafic-volcaniclastic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-ultramafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed ultramafic volcaniclastic rock consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin and with an overall mafic composition, but otherwise uncategorized. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">muv</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">meta-ultramafic volcaniclastic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metabasalt">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A fine-grained metamorphosed mafic volcanic rock consisting essentially of calcic plagioclase and pyroxene. Igneous protolith defined modally in QAPF fields 9 and 10 (Le Maitre et al., 2005, p. 31). GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mbb</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metabasalt</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metabasaltic-andesite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A fine-grained metamorphosed mafic volcanic rock with plagioclase of variable composition and the ferromagnesian minerals more commonly found in basalts, e.g. olivine. Igneous protolith defined chemically in TAS field O1 (Le Maitre et al., 2005, p. 61). GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mbd</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metabasaltic andesite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metacarbonate-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-carbonate"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock consisting essentially of carbonate minerals and derived from a carbonate protolith, but otherwise uncategorized.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mk</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metacarbonate rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metacarbonate-with-lesser-siliciclastic">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-carbonate"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Metamorphosed carbonate rocks interleaved at outcrop scale with subordinate metamorphosed siliciclastic rocks.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mkt</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metacarbonate with lesser siliciclastic</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metacarbonatite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A collective term for a metamorphosed igneous (intrusive, extrusive or volcaniclastic) rock in which the protolith had a modal amount of primary carbonate minerals greater than 50%, but otherwise uncategorized (section 2.3, p. 10). (Cf. Le Maitre et al., 2005, p. 68)</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mq</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metacarbonatite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metacarbonatite-granofels-hornfels">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-carbonatite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A granofels/hornfels derived by (contact) metamorphism of a carbonatite, i.e. an igneous protolith containing a modal amount of primary carbonate minerals greater than 50%. See also definition of granofels/granofelsic structure in Fettes and Desmons (2007, p. 156); hornfels are granofels formed by contact metamorphism (Fettes and Desmons, 2007, p. 159).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mqe</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metacarbonatite granofels/hornfels</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metachert">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-other-chemical"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed chert.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mc</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metachert</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metaconglomerate">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-siliciclastic-psephite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock derived from a siliciclastic sedimentary protolith with grain size greater than 2 mm, i.e. coarser than sand.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mx</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metaconglomerate</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/psephite</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-siliciclastic-psephite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A group name for metamorphosed rocks derived from a sedimentary siliciclastic protolith with grain size greater than 2 mm, i.e. the metamorphosed equivalent of a conglomerate. Acknowledging entrenched practises, GSWA usage allows the use of psephite as a noun for a metamorphosed equivalent of conglomerate lithologies (i.e. grain size greater than 2 mm), as well as an adjective to qualify metamorphic rocks derived from these sedimentary types (e.g. psephitic schist).</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mx</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metasedimentary siliciclastic: psephite</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/psephite</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">psephyte</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metaconglomerate"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/mylonitized-metaconglomerate"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/psephitic-gneiss"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/psephitic-granofels-hornfels"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/psephitic-schist"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/silicified-metaconglomerate"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metadacite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed volcanic rock derived from a protolith composed of quartz and sodic plagioclase with minor amounts of biotite and/or hornblende and/or pyroxene (volcanic equivalent of granodiorite and tonalite). Igneous protolith defined modally in QAPF fields 4 and 5, and, if modes are not available, chemically in TAS field O3 (Le Maitre et al., 2005, p. 72). GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mfd</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metadacite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metadiorite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic intrusive rock derived from a protolith consisting of intermediate plagioclase, commonly with hornblende and often with biotite or augite. Igneous protolith defined modally in QAPF field 10 (Le Maitre et al., 2005, p. 73).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mgi</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metadiorite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metadolerite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed mafic intrusive rock (or coarse-grained part of extrusive successions) derived from a protolith intermediate in grain size between basalt and gabbro and composed essentially of plagioclase, pyroxene and opaque minerals with varying amounts of quartz and possibly olivine; often with ophitic texture. Igneous protolith defined modally in QAPF (Le Maitre et al., 2005, p. 74).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mod</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metadolerite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metadunite-derived-from-intrusive-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-ultramafic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed ultramafic igneous (intrusive or coarse-grained extrusive) rock consisting essentially of olivine. Igneous protolith defined modally in the ultramafic rock classification of Le Maitre et al. (2005, p. 28).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mad</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metadunite derived from intrusive rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metadunite-derived-from-volcanic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-ultramafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed ultramafic volcanic rock consisting essentially of olivine. Igneous protolith defined modally in the ultramafic rock classification (cf. Le Maitre et al., 2005, p. 74). In GSWA usage this term refers to a metamorphosed dunite identified as formed from fractionation within thick bodies of ultramafic volcanic or subvolcanic magmas. GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mud</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metadunite derived from volcanic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metafelsic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic rock of igneous but otherwise unspecified origin, i.e. a felsic rock for which an igneous protolith (intrusive, extrusive or volcaniclastic) is interpreted but commonly too weathered or altered to make further distinctions.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mr</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metafelsic rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/felsic</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metafelsic-volcanic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock consisting essentially of felsic minerals and derived from a felsic volcanic protolith, but otherwise uncategorized. GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mf</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metafelsic volcanic rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/felsic</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metafelsic-volcaniclastic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed volcaniclastic rock consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin and with an overall felsic composition, but otherwise uncategorized. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mfv</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metafelsic volcaniclastic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metagabbro">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed intrusive rock (or coarse-grained parts of extrusive successions), derived from a protolith composed essentially of calcic plagioclase, pyroxene and iron oxides, with varying amounts of quartz and possibly olivine. Igneous protolith defined modally in QAPF field 10 (cf. Le Maitre et al., 2005, p. 83).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mog</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metagabbro</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metagabbronorite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed mafic intrusive rock (or coarse-grained parts of extrusive successions) derived from a protolith consisting of calcic plagioclase and roughly equal amounts of clinopyroxene and orthopyroxene. Igneous protolith defined modally in the gabbroic rock classification (Le Maitre et al., 2005, p. 83).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mom</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metagabbronorite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metagranitic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">In GSWA usage, a generic term for a metamorphosed rock derived from an igneous intrusive protolith composed chiefly of one or more felsic, light-coloured minerals in its mode and with quartz modal content between 0 and 60%, otherwise uncategorized.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mg</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metagranitic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metagranodiorite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic intrusive rock derived from a protolith consisting essentially of quartz, sodic plagioclase and lesser amounts of alkali feldspar with minor amounts of hornblende and biotite. Igneous protolith defined modally in QAPF field 4 (Le Maitre et al., 2005, p. 86).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mgg</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metagranodiorite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metahydrothermal-chert">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphosed-hydrothermal"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed, fine-grained siliceous rock that precipitated originally from a hydrothermal fluid related to seafloor or sub-seafloor volcanism.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mzc</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metahydrothermal chert</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">Adachi</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">Hydrothermal chert and associated siliceous rocks from the northern Pacific their geological significance as indication od ocean ridge activity: Sedimentary Geology</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">K</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">M</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">R 1986</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">Sugisaki</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">Yamamoto</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">doi.org/10.1016/0037-0738(86)90075-8.</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://doi.org/10.3390/min12101333</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">p. 125-148</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">vol. 47(1–2)</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metakomatiite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-ultramafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed ultramafic extrusive rock derived from lavas that crystallize from high temperature magmas with 18% to 32% MgO. Pillows, chilled flow-tops and spinifex textures are commonly preserved. Igneous protolith defined chemically in the TAS classification (cf. Le Maitre et al., 2005, p. 98).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">muk</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metakomatiite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metakomatiitic-basalt">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed member of the komatiite series derived from a volcanic rock characterized by MgO in the range 5–15%, typically displaying spinifex texture and evidence of rapid quenching of high-temperature magma (cf. Le Maitre et al., 2005, p. 61) and this vocabulary for a definition of the igneous protolith. GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mbm</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metakomatiitic basalt</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metalatite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock derived from a protolith chemically lying between trachyte and andesite, i.e. derived from a volcanic rock composed of approximately equal amounts of alkali feldspar and sodic plagioclase. Igneous protolith defined modally in QAPF field 8 and, if modes are not available, chemically as the potassic variety of trachyandesite in TAS field S3 (Le Maitre et al., 2005, p. 101). GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mfl</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metalatite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamafic-intrusive-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock consisting essentially of mafic minerals and derived from a mafic intrusive protolith or from coarse-grained parts of extrusive successions, but otherwise uncategorized.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mo</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamafic intrusive rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamafic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed mafic rock, i.e. the metamorphosed equivalent of an igneous intrusive, extrusive or volcaniclastic rock mainly consisting of mafic minerals for which an igneous protolith is interpreted but commonly too weathered or altered to make further distinctions (cf. Fettes and Desmons, 2007, p.170).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007) and GSWA usage.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mw</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamafic rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/mafic</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamafic-volcanic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock consisting essentially of mafic minerals and derived from a mafic volcanic protolith, but otherwise uncategorized. GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mb</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamafic volcanic rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/mafic</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamafic-volcaniclastic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed volcaniclastic rock consisting of either (a) pyroclastic detritus or (b) terrigenous volcanic detritus of epiclastic origin and with an overall mafic composition, but otherwise uncategorized. &apos;Volcaniclastic&apos; includes all clastic volcanic materials formed by any process of fragmentation, dispersed by any kind of transporting agent, deposited in any environment, or mixed in any significant portion with non-volcanic fragments.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mbv</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamafic volcaniclastic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamonzodiorite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic intrusive rock derived from a protolith intermediate between monzonite and diorite. Igneous protolith defined modally in QAPF field 9 (Le Maitre et al., 2005, p. 113).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mgh</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamonzodiorite</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/monzodiorite</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamonzogranite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic intrusive rock derived from a monzogranite, i.e. from a variety of granite in QAPF field 3b having roughly equal amounts of alkali feldspar and plagioclase (cf. Le Maitre et al., 2005, p. 113).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mgm</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamonzogranite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamonzonite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed felsic intrusive rock derived from a monzonite, i.e. an intrusive rock containing almost equal amounts of plagioclase and alkali feldspar with minor amphibole and/or pyroxene. Igneous protolith defined modally in QAPF field 8 (Le Maitre et al., 2005, p. 113).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mgz</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamonzonite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphic rock so deeply altered/weathered that no inference can be made as to the protolith and with no discernible structures.</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/metamorphicRock"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/metamorphic_rock"/>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">m</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamorphic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A group name for metamorphic rocks so deeply altered/weathered that no inference can be made as to the protolith and with no discernible structures.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">m</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamorphic</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphic-rock"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphosed-hydrothermal-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metamorphosed-hydrothermal"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A hydrothermal rock that has undergone metamorphism, otherwise uncategorized.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mz</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metamorphosed hydrothermal rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metanorite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-mafic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed mafic intrusive rock (or coarse-grained parts of extrusive successions) derived from a norite, i.e. a rock composed essentially of bytownite, labradorite or andesine and orthopyroxene. Igneous protolith defined modally in the gabbroic rock classification (Le Maitre et al., 2005, p. 117).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mow</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metanorite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metapegmatite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The metamorphosed equivalent of a pegmatite, i.e. an exceptionally coarse-grained igneous rock with interlocking crystals, usually found as irregular dykes, lenses, or veins, especially at the margins of batholiths and of granitic composition (cf. Le Maitre et al., 2005, p. 125).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mgp</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metapegmatite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metaperidotite-derived-from-intrusive-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-ultramafic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed ultramafic igneous (intrusive or coarse-grained extrusive) rock consisting essentially of olivine, pyroxene and/or amphibole. Igneous protolith defined modally in the ultramafic rock classification of Le Maitre et al. (2005, p. 28).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">map</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metaperidotite derived from intrusive rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metaperidotite-derived-from-volcanic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-ultramafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A collective term for metamorphosed ultramafic volcanic rocks derived from a protolith consisting essentially of olivine, pyroxene and/or amphibole. Igneous protolith defined modally in the ultramafic rock classification of Le Maitre et al. (2005, p. 28 and 126). In GSWA usage this term refers to a metamorphosed peridotite identified as formed from fractionation within thick bodies of ultramafic volcanic or subvolcanic magmas.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mup</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metaperidotite derived from volcanic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metapyroxenite-derived-from-intrusive-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-ultramafic-intrusive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Metamorphic rock consisting of more than 90% modal pyroxene (Fettes and Desmons, 2007, p. 190), i.e. a metamorphosed ultramafic igneous (intrusive or coarse-grained extrusive) rock composed almost entirely of one or more pyroxenes and occasionally biotite, hornblende and olivine. Igneous protolith defined modally in the ultramafic rock classification of Le Maitre et al. (2005, p. 28).</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">max</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metapyroxenite derived from intrusive rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metapyroxenite-derived-from-volcanic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-ultramafic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed ultramafic volcanic or volcaniclastic rock derived from a protolith composed almost entirely of one or more pyroxenes (greater than 90% modal pyroxenes) and occasionally biotite, hornblende and olivine; igneous protolith defined modally in the ultramafic rock classification (cf. Le Maitre et al., 2005, p. 28 and 134; Fettes and Desmons, 2007, p. 190). In GSWA usage this term refers to a metamorphosed pyroxenite identified as formed from fractionation within thick bodies of ultramafic volcanic or subvolcanic magmas.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mux</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metapyroxenite derived from volcanic rock</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metarhyodacite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed volcanic rock derived from a protolith intermediate between rhyolite and dacite. Igneous protolith defined as consisting of phenocrysts of quartz, plagioclase and a few ferromagnesian minerals in a microcrystalline groundmass (Le Maitre et al., 2005, p. 137). GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mfc</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metarhyodacite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metarhyolite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/meta-igneous-felsic-volcanic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed silicic volcanic rocks derived from a rhyolitic protolith, i.e. a rock consisting of phenocrysts of quartz and alkali feldspar, often with minor plagioclase and biotite, in a microcrystalline or glassy groundmass and having the chemical composition of granite. Igneous protolith defined modally in QAPF field 3 and, if modes are not available, chemically in TAS field R (Le Maitre et al., 2005, p. 137). GSWA&apos;s rock classification scheme prefers the use of volcanic terminology for any igneous rock formed on or near the Earth&apos;s surface, i.e. for those hypabyssal or subvolcanic rocks such as dykes, sills, and cryptodomes that are demonstrably both spatially and temporally associated with extrusive volcanic rocks.</skos:definition>
    <skos:historyNote>Based on the IUGS classification of metamorphic (Fettes and Desmons, 2007) and igneous rocks (Le Maitre et al., 2005).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mfr</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metarhyolite</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasandstone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-siliciclastic-psammite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock derived from a siliciclastic sedimentary protolith with grain size between 0.0625 and 2 mm.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">mt</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metasandstone</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock of sedimentary but otherwise unspecified origin, i.e. a rock for which a clastic or chemical/biochemical derivation cannot be conclusively established (e.g. due to weathering ).</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">msed</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metasedimentary rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/sedimentary</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A group name for metamorphosed rocks of sedimentary but otherwise unspecified origin, i.e. a rock for which a clastic or chemical/biochemical derivation cannot be conclusively established.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">msed</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metasedimentary</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/sedimentary</schema:citation>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-rock"/>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasiliciclastic-rock">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-siliciclastic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock derived from a siliciclastic protolith, i.e. from silicon-bearing (in the form of quartz and silicates), clastic non-carbonate rocks, but otherwise uncategorized. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011)</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">md</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metasiliciclastic rock</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/clastic</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/siliciclastic</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasiltstone-metamudstone-metashale">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-siliciclastic-pelite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A metamorphosed rock derived from a siliciclastic sedimentary protolith with grains less than 0.0625 mm in size.</skos:definition>
    <skos:historyNote>Compiled by the Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">ml</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metasiltstone/metamudstone/metashale</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metasedimentary-siliciclastic-pelite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A group name for metamorphosed rocks derived from a fine-grained sedimentary siliciclastic protolith (usually with grain size below 0.0625 mm), i.e. the metamorphosed equivalent of a mudstone. Fettes and Desmons (2007, p. 181) identify pelite as a restricted-use term, and define it as &quot;... a term for a sedimentary rock composed of clay-sized particles. It has also been used locally for the metamorphosed equivalent of these sedimentary rocks, that is, a metamorphic rock with a high modal ratio of mica to quartz+feldspar. The SCMR recommends that it should be used only for sedimentary rocks and the term metapelite should be used for the metamorphosed equivalents.&quot; Acknowledging entrenched practises, GSWA usage departs from this recommendation, and allows the use of pelite as a noun for a metamorphosed equivalent of mudstone lithologies, as well as an adjective to qualify metamorphic rocks derived from these sedimentary types (e.g. pelitic schist).</skos:definition>
    <skos:historyNote>Partly based on the IUGS classification of metamorphic rocks (Fettes and Desmons, 2007).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">ml</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">metasedimentary siliciclastic: pelite</skos:prefLabel>
