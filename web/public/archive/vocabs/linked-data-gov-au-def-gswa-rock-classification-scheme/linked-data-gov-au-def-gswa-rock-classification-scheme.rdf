    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/mafic-volcaniclastic-breccia-sandstone"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/mafic-volcaniclastic-conglomerate"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/mafic-volcaniclastic-granule-pebble-breccia"/>
