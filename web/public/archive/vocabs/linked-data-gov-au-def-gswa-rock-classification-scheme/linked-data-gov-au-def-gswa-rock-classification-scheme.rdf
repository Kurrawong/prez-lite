  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/anhydrite-evaporite">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/evaporite"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An evaporite consisting predominantly of the mineral anhydrite, i.e. of anhydrous calcium sulfate (CaSO₄).</skos:definition>
