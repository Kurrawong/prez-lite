  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/claystone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/mudstone"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An indurated mud with 66% or more clay-sized minerals, and the composition of shale but lacking its fine lamination or fissility; a massive mudstone in which clay predominates over silt; a nonfissile clay shale. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011).</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/claystone"/>
    <skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/claystone"/>
    <skos:historyNote>Pettijohn, FJ 1957, Sedimentary rocks, Harper and Brothers, New York, 718p.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:notation rdf:datatype="https://linked.data.gov.au/def/gswa-rock-classification-scheme/gswa-rock-code">sly</skos:notation>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">claystone</skos:prefLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/claystonesed</schema:citation>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">argillite</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/gswa-rock-classification-scheme/mudstone">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/sedimentary-siliciclastic"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">An indurated mud having the texture and composition of shale, but lacking its fine lamination or fissility. A general term that includes clay, silt, claystone, siltstone, shale, and argillite, and that should be used only when the amounts of clay and silt are not known or specified or cannot be precisely identified; or when it is desirable to characterize the whole family of finer-grained sedimentary rocks (as distinguished from sandstones, conglomerates, and limestones). The term specifies neither mineralogic composition nor mud of clastic origin. Historical GSWA usage has incorrectly used mudstone interchangeably with siltstone. Adapted from Glossary of geology (5th edition revised; Neuendorf et al., 2011).</skos:definition>
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/clasticMudstone"/>
