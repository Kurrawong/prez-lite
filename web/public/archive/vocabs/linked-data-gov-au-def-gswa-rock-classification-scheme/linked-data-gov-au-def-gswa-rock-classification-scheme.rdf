    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metaperidotite-derived-from-volcanic-rock"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/metapyroxenite-derived-from-volcanic-rock"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/mylonitized-meta-ultramafic-rock-volcanic-or-undivided"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/gswa-rock-classification-scheme/serpentinite-derived-from-volcanic-rock"/>
