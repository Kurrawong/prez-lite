    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/lost"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/on-display"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/processing"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/quarantined"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/collected"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/stolen"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/temporary-holding"/>
    <schema:creator rdf:resource="https://linked.data.gov.au/org/gswa"/>
