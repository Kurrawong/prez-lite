  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/awaiting-registration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample flagged for permanent archiving, but currently in temporary storage awaiting registration.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">awaiting registration</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/consumed">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">No part or amount of the sample or specimen remains extant as a result of intentional (e.g. analysis or processing) events. This excludes any child materials developed from the original destroyed sample. Any corresponding records in the collection management system(s) should be archived.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">consumed</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/deaccessioned"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/deaccessioned">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample or specimen removed from storage or not retained permanently, based on a perceived lack of space, the material&apos;s unsuitability for storage (e.g. hazardous), poor condition, and/or limited utility or applicability to ongoing studies. Any corresponding records in the collection management system(s) should be archived.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">deaccessioned</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/consumed"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/destroyed"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/discarded"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/donated"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/restituted"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/destroyed">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">No part or amount of the sample or specimen remains extant as a result of unintentional (e.g. accidental damage) events. This excludes any child materials developed from the original destroyed sample. Any corresponding records in the collection management system(s) should be archived.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">destroyed</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/deaccessioned"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/discarded">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">disposed of</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample or specimen has been intentionally removed from formal collections due to a perceived lack of space, the material&apos;s unsuitability for storage (e.g. hazardous), poor condition, and/or limited utility or applicability to ongoing studies (e.g. unknown origin/provenance). Disposed material is generally placed in the trash or informally donated due to a lack of value. Any corresponding records in the collection management system(s) should be archived.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">discarded</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/deaccessioned"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/donated">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample or specimen has been permanently transferred to another organization through mutual agreement or arrangement. Any corresponding records in the collection management system(s) should be archived.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">donated</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/deaccessioned"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/in-transit">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The sample or specimen is being moved between locations.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/sample-location-status/in-transit"/>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">in transit</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/loaned-in">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample or specimen brought in temporarily from an outside source or organization under a limited-term agreement.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">loaned in</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/loan"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/loan">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">borrowed</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample or specimen moved outside its registered storage facility on the basis of a formal agreement with a person or institution, usually for display or study purposes.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">loan</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/loaned-in"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/long-term-loan"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/on-loan-external"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/on-loan-internal"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/permanent-loan"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/long-term-loan">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample or specimen is outside its registered storage facility on the basis of a loan agreement with an external researcher or organization, where that agreement has a longer than standard duration. This is common for material loaned for display in static galleries, where the loan may be reviewed and renewed periodically in lieu of a fixed-term agreement.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">long-term loan</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/loan"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/lost">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Location of sample or specimen is not known. Any corresponding records in the collection management system(s) should be preserved, and retrieved should the sample/specimen be recovered.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/sample-location-status/lost"/>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">lost</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/on-display">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">exhibited</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample or specimen held in a museum or other location for display, education and/or demonstration purposes. Displays can be located indoors or outdoors. May refer to specimens on loan that are on public display.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/sample-location-status/on-display"/>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">on display</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/on-loan-external">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">loaned out</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample or specimen is outside its registered storage facility on the basis of a limited-term agreement with an external researcher or organization.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">on loan - external</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/loan"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/on-loan-internal">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample or specimen is outside its registered storage facility on the basis of a limited-term agreement with an internal staff member.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">on loan - internal</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/loan"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/permanent-loan">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample or specimen is outside its registered storage facility on the basis of an agreement with an external researcher or organization, where there is no time limit set on the loan&apos;s duration and no program of loan review. Permanent loans are similar to donations, except that the loaning organization retains the right to recall the sample/specimen if required.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">permanent loan</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/loan"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/processing">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The sample or specimen is at a laboratory, university, or other facility external to its registered storage facility to undergo processing or analysis.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/sample-location-status/processing"/>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">processing</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/quarantined">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample or specimen placed in temporary or permanent isolation due to suspected or proven contamination or safety concerns. Isolation can be either spatial (separate cupboard) or engineered (suitable storage material, e.g. lead shield).</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">quarantined</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/restituted">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">repatriated</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample or specimen that has been returned to the country or community of origin along with the unconditional rights to that object. This may include digital restitution, i.e. where only images and documentations are returned to the country or community of origin.</skos:definition>
    <skos:historyNote>Definition adpted from https://www.nma.gov.au/about/publications/repatriation-handbook/chapter-1.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">restituted</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/deaccessioned"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/sampled-under-agreement">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample collected from material (such as core or cuttings) held in the organization&apos;s collections as part of a departmental agreement or permit, normally by an external party for destructive analysis and research. Sampled-under-agreement also applies to any derivative samples/specimens and remnant materials, as well as related data, images and reports â€” the return/submission of which is a mandatory requirement of the agreement.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">sampled under agreement</skos:prefLabel>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/sample-curation-status/collected"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/collected">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample collected from a sampling location and held in temporary storage prior to examination, analysis, or transfer to permanent or semi-permanent storage.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/sample-location-status/collected"/>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">collected</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/sample-curation-status/sampled-under-agreement"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/stolen">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample or specimen removed without permission or legal right and without any intention of return, and its whereabouts are unknown. Any corresponding records in the collection management system(s) should be preserved, and retrieved should the sample/specimen be recovered.</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">stolen</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/temporary-holding">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample or specimen that is being stored on a temporary basis, either for a set period or until a specific condition is met. This holding may be in preparation for destruction, transfer to another organization, or as a result of legislative or legal processes (such as confiscation of illegally obtained materials or evidence for an investigation).</skos:definition>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">temporary holding</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

