  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status/archived">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">in storage</skos:altLabel>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A sample or specimen held at a storage location in an archived state, i.e. the sample/specimen has been fully assessed, prepared and recorded.</skos:definition>
    <skos:exactMatch rdf:resource="http://linked.data.gov.au/def/sample-location-status/in-storage"/>
    <skos:historyNote>Compiled by Geological Survey of Western Australia</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">archived</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/sample-curation-status"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/sample-curation-status">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The status of a sample or specimen in relation to curation processes (e.g. the recording and organization of items in a collection), both before and after storage/archiving, and with respect to its display or storage location at a specific point in time.</skos:definition>
    <skos:historyNote xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Compiled by the Geological Survey of Western Australia to assist in the management of its physical sample/specimen collections. GSWA distinguishes samples from specimens on the basis of the latter including both primary and derived materials and having been accessioned into a suitable management system.</skos:historyNote>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Sample/specimen curation status</skos:prefLabel>
    <http://purl.org/linked-data/registry#status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <owl:versionIRI rdf:resource="https://linked.data.gov.au/def/sample-curation-status/1"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/archived"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/awaiting-registration"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/deaccessioned"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/sample-curation-status/in-transit"/>
