    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/fault-controlled"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration associated with a fault displaying angular rock fragments resulting from fracturing.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">fault breccia</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/fault-controlled">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/fracture-controlled"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration spatially associated with a fault.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">fault-controlled</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/fault-breccia"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/foliation-controlled">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/structure-controlled"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration confined to volumes of rocks exhibiting a foliation.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">foliation-controlled</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">foliation parallel</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/structure-controlled">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/non-pervasive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration spatially restricted to rocks within and/or immediately adjacent to structures such as shear zones, fault zones, veins, hydrothermal breccias, vugs or fractures.</skos:definition>
    <skos:historyNote>Definition from Pirajno (2009; Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">structure-controlled</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/hydrothermal-breccia-controlled"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/foliation-controlled"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/shear-zone-controlled"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/vein-controlled"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/vug-controlled"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/fracture-controlled"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/pervasive">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration characterized by the replacement of most, or all, original rock-forming minerals and obliteration of textures at the scale of observation.</skos:definition>
    <skos:historyNote>Definition from:
1) Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p. (p. 95)
2) Thompson, JFH, Thompson, AJB and Allen, RL 1996, Atlas of alteration: A field and petrographic guide to hydrothermal alteration minerals, St. John&apos;s, Nfld: Geological Association of Canada, Mineral Deposits Division, 119p. (p. 3).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">pervasive</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">complete</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">continuous</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">extensive</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">homogeneous</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">massive</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">ubiquitous</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">widespread</skos:altLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/selectively-non-pervasive">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/selective"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration characterized by the replacement of only specific components (e.g. minerals, grains, clasts) of the original rock, with partial to complete obliteration of textures only in part of the area under observation.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">selectively non-pervasive</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/selective">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration characterized by the replacement of only specific components (e.g. minerals, grains, clasts) of the original rock, with partial to complete obliteration of textures at the scale of observation.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">selective</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/selectively-non-pervasive"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/selectively-pervasive"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/selectively-pervasive">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/selective"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration characterized by the replacement of only specific components (e.g. minerals, grains, clasts) of the original rock, with partial to complete obliteration of textures across the whole area of observation.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">selectively pervasive</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/shear-zone-controlled">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/structure-controlled"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration spatially associated with a shear zone.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">shear zone-controlled</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/unaltered">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A rock with no visible signs of alteration, in which primary minerals, fabrics (including primary pore space) and chemistry are unchanged.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">unaltered</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">not altered</skos:altLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/alteration-intensity/unaltered"/>
    <skos:exactMatch rdf:resource="https://linked.data.gov.au/def/alteration-type/unaltered"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/unconformity-related">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/layer-controlled"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration that occurs at or around an unconformable or disconformable contact.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">unconformity-related</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/vein-controlled">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/structure-controlled"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration spatially associated with a vein.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">vein-controlled</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">stockwork</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">veined</skos:altLabel>
