    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration restricted to compositionally or texturally defined layers within rocks.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">layer-controlled</skos:prefLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/banding-controlled"/>
