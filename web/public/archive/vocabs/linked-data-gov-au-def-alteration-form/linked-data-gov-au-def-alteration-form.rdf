  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/fracture-controlled">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/structure-controlled"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration spatially associated with a fracture, i.e. with any break/separation in a geological formation with or without relative displacement of the parts (e.g. a joint or a fault).</skos:definition>
    <skos:historyNote>Definition from Pirajno (2009; Pirajno, F 2009, Hydrothermal processes and mineral systems: Springer-Verlag, 1250p).</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">fracture-controlled</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">fracture parallel</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/fault-controlled"/>
  </rdf:Description>

