  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/vug-controlled">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/structure-controlled"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration associated with small cavities in a vein or rock, usually lined with crystals of a different mineral composition from the enclosing rock.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
