  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/zoned">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:broader rdf:resource="https://linked.data.gov.au/def/alteration-form/non-pervasive"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Alteration that can be divided spatially into several mineralogically or texturally distinct regions, with systematic variations in mineral assemblages, compositions and/or textures.</skos:definition>
    <skos:historyNote>Definition compiled by the Geological Survey of Western Australia.</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/alteration-form"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">zoned</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">aureole</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">selvage</skos:altLabel>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/zoned-central"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/zoned-inner"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/alteration-form/zoned-outer"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/alteration-form/zoned-inner">
