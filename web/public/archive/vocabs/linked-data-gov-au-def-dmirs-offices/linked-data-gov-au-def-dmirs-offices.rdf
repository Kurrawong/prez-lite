  <rdf:Description rdf:about="https://linked.data.gov.au/def/dmirs-offices/carlisle">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/dmirs-offices"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Carlisle is one of several offices maintained by the Department of Energy, Mines, Industry Regulation and Safety within the Perth metropolitan area. The site includes several buildings encompassing separate functions of the Geological Survey of Western Australia, including the Perth Core Library, and is located at 37 Harris St, Carlisle WA 6101.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/dmirs-offices"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Carlisle</skos:prefLabel>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.dmirs.wa.gov.au/content/metropolitan-offices</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/dmirs-offices/joe-lord-core-library">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/dmirs-offices"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The JH (Joe) Lord Core Library is one of two Geological Survey storage and viewing facilities housing geological exploration materials (predominantly drillcores and samples) and related media. The building is on the Kalgoorlie site at the corner of Hunter and Broadwood Streets, West Kalgoorlie WA 6430.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/dmirs-offices"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">JH (Joe) Lord Core Library</skos:prefLabel>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Joe Lord Core Library</skos:altLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Kalgoorlie Core Library</skos:altLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://dmp.wa.gov.au/Geological-Survey/Core-library-services-and-1391.aspx</schema:citation>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/dmirs-offices/kalgoorlie-office">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/dmirs-offices"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">The Kalgoorlie office is one of several regional offices maintained by the Department of Energy, Mines, Industry Regulation and Safety, representing the department in the Goldfields and Esperance regions. In addition to administrative functions of the Department and of the Geological Survey of Western Australia, the Kalgoorlie office houses the Joe (JH) Lord Core Library, East Mine Inspectorate and Kalgoorlie Mining Registrar. The office is at the corner of Hunter and Broadwood Streets, West Kalgoorlie WA 6430.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/dmirs-offices"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Goldfields–Esperance (Kalgoorlie) regional office</skos:prefLabel>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Kalgoorlie office</skos:altLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://waresources.sharepoint.com/sites/intranet/sitepages/regional-offices-&amp;-indian-ocean-territories.aspx</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.dmirs.wa.gov.au/content/regional-offices</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.dmp.wa.gov.au/Safety/Regional-inspectorate-boundaries-9872.aspx</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.dmp.wa.gov.au/History-and-locations-of-the-12531.aspx</schema:citation>
    <skos:closeMatch rdf:resource="https://linked.data.gov.au/def/gswa-collection-facilities/kalgoorlie-regional-office"/>
