    <owl:versionIRI rdf:resource="https://linked.data.gov.au/def/dmirs-offices/1.1"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/dmirs-offices/dmirs-metropolitan"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/dmirs-offices/dmirs-regional"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/dmirs-offices/wa-core-libraries"/>
    <schema:creator rdf:resource="https://linked.data.gov.au/org/gswa"/>
    <schema:dateCreated rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-09-13</schema:dateCreated>
    <schema:dateModified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-01</schema:dateModified>
    <schema:publisher rdf:resource="https://linked.data.gov.au/org/gswa"/>
    <schema:version>1.1</schema:version>
    <schema:keywords rdf:resource="https://linked.data.gov.au/def/gswa-vocabulary-themes/administration"/>
