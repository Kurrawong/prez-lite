  <rdf:Description rdf:about="https://linked.data.gov.au/def/dmirs-offices/mineral-house">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/dmirs-offices"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Mineral House is one of several offices maintained by the Department of Energy, Mines, Industry Regulation and Safety within the Perth metropolitan area. It is the department&apos;s main administrative office, housing the Geological Survey and Resource Strategy, Resource and Environmental Compliance, Resource Tenure and Aboriginal Empowerment Petroleum, Mineral Titles Divisions of the Department, the West Mine Inspectorate and the Perth Mining Registrar. The building is at 100 Plain St, East Perth WA 6004.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/dmirs-offices"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Mineral House</skos:prefLabel>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">MH</skos:altLabel>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.dmirs.wa.gov.au/content/metropolitan-offices</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.dmp.wa.gov.au/Safety/Regional-inspectorate-boundaries-9872.aspx</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.dmp.wa.gov.au/History-and-locations-of-the-12531.aspx</schema:citation>
