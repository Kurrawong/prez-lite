>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/directional">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://coringmagazine.com/article/directional-diamond-drilling-definitions-terms-simple-calculations/</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/directionaldrilling</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.slb.com/en/terms/d/directional_well</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://linked.data.gov.au/def/borehole-design/deviated</schema:citation>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:closeMatch rdf:resource="https://linked.data.gov.au/def/borehole-design/deviated"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole intentionally drilled at controlled departures from the vertical or parent borehole trajectory and at controlled azimuths, achieved with the use of geosteering equipment or via other methods (e.g. directional wedge). The main purpose is usually to establish multiple boreholes from a single location and/or to intersect a target or zone of interest. Note that the term &apos;deviated&apos; is not recommended as an alternate label of &apos;directional&apos;, as it can be used to refer to a borehole that unintentionally deviates from the intended trajectory...</skos:definition>
    <skos:historyNote>Energy and mineral industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-configuration/extended-reach"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-configuration/lateral"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">directional</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A description of the intended configuration and trajectory of a borehole, covering concepts from the energy and mineral resources industry. Note that most of the concepts in this vocabulary apply to both the process of drilling (e.g. directional drilling), as well as to an actual borehole (e.g. directional well), even if the definitions in this vocabulary focus specifically on the latter.</skos:definition>
    <skos:historyNote xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Concepts in this vocabulary are those used by the energy and mineral resources industry in Western Australia. Definitions and terms expand on those in the Geological Survey of Queensland&apos;s Borehole design vocabulary (https://linked.data.gov.au/def/borehole-design).</skos:historyNote>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Borehole configuration</skos:prefLabel>
    <schema:dateCreated rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-09-01</schema:dateCreated>
    <schema:creator rdf:resource="https://linked.data.gov.au/org/gswa"/>
    <schema:dateModified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-09-01</schema:dateModified>
    <schema:publisher rdf:resource="https://linked.data.gov.au/org/gswa"/>
    <owl:versionIRI rdf:resource="https://linked.data.gov.au/def/borehole-configuration/1"/>
    <owl:versionInfo>1</owl:versionInfo>
    <http://purl.org/linked-data/registry#status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
    <schema:keywords rdf:resource="https://linked.data.gov.au/def/gswa-vocabulary-themes/resources"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/borehole-configuration/directional"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/borehole-configuration/horizontal"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/borehole-configuration/inclined"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/borehole-configuration/vertical"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/borehole-configuration/vertical-uphole"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/extended-reach">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.slb.com/en/terms/e/extended-reach_drilling</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://petrowiki.spe.org/Extended_reach_wells</schema:citation>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A directional well in which the ratio of the measured depth (MD) to the true vertical depth (TVD) is at least 2:1.</skos:definition>
    <skos:historyNote>Energy industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">extended-reach</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/lateral">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.americangeosciences.org/term/lateraldrill</schema:citation>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/borehole-design/lateral"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled with geosteering equipment with the intent of controlling the trajectory to drill laterally within a targeted stratigraphic interval or intervals. Usually, but not always, drilled close to horizontal and for relatively short distances.</skos:definition>
    <skos:historyNote>Energy industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">lateral</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/horizontal">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://glossary.slb.com/en/terms/h/horizontal_drilling</schema:citation>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://petrowiki.spe.org/Horizontal_wells</schema:citation>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled in a horizontal direction. In the energy industry, this is achieved with geosteering equipment and applies to boreholes where the departure from vertical exceeds about 80°. In the mineral industry, horizontal drilling is typically carried out from an underground location or on the wall of an open pit.</skos:definition>
    <skos:historyNote>Resources industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">horizontal</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:relatedMatch rdf:resource="http://linked.data.gov.au/def/borehole-design/lateral"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/inclined">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled from the start point at a pre-set nonvertical angle above or below horizontal.</skos:definition>
    <skos:historyNote>Mineral industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-configuration/inclined-down"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-configuration/inclined-up"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">inclined</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">angled</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/inclined-down">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/borehole-design/inclineddown"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled from the start point at a pre-set nonvertical angle below horizontal (drilled downwards).</skos:definition>
    <skos:historyNote>Mineral industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">inclined down</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">downhole</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/inclined-up">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/borehole-design/inclinedup"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled from the start point at a pre-set nonvertical angle above horizontal (drilled upwards).</skos:definition>
    <skos:historyNote>Mineral industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">inclined up</skos:prefLabel>
    <skos:altLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">uphole</skos:altLabel>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/vertical">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://coringmagazine.com/article/directional-diamond-drilling-definitions-terms-simple-calculations/</schema:citation>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/borehole-design/vertical"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled in a vertical downwards direction with the intent of maintaining a vertical trajectory. Deviations up to 2° from the vertical line are usually accepted as &apos;vertical’.</skos:definition>
    <skos:historyNote>Resources industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">vertical</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-configuration/vertical-uphole">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">A borehole drilled underground from the start point in a pre-set vertical upwards direction.</skos:definition>
    <skos:historyNote>Resources industry concept</skos:historyNote>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">vertical uphole</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-configuration"/>
  </rdf:Description>

