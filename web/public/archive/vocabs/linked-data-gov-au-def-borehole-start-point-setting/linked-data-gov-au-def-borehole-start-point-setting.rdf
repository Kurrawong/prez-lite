  <rdf:Description rdf:about="http://linked.data.gov.au/def/borehole-start-point/from-pre-existing-hole">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://linked.data.gov.au/def/borehole-start-point/from-pre-existing-hole</schema:citation>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-start-point"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates that the borehole was drilled from a pre-existing hole, whether at surface or underground.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">from pre-existing hole</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-start-point-setting">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates the nature of the borehole start point location, i.e. the general setting and type of surrounding where the borehole is positioned (e.g. onshore, offshore, etc.).</skos:definition>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Borehole start point setting</skos:prefLabel>
    <skos:historyNote xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Terms and definitions are predominantly sourced from the Geological Survey of Queensland&apos;s (GSQ) Borehole start point vocabulary (http:/linked.data.gov.au/def/borehole-start-point), with some modifications and additional concepts.</skos:historyNote>
    <schema:dateCreated rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-09-06</schema:dateCreated>
    <schema:creator rdf:resource="https://linked.data.gov.au/org/gswa"/>
    <schema:dateModified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-09-06</schema:dateModified>
    <schema:publisher rdf:resource="https://linked.data.gov.au/org/gswa"/>
    <owl:versionIRI rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting/1"/>
    <owl:versionInfo>1</owl:versionInfo>
    <http://purl.org/linked-data/registry#status rdf:resource="https://linked.data.gov.au/def/reg-statuses/stable"/>
