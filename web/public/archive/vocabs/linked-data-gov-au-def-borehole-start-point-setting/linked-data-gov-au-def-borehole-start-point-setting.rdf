    <skos:hasTopConcept rdf:resource="http://linked.data.gov.au/def/borehole-start-point/underground"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting/offshore"/>
    <skos:hasTopConcept rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting/onshore"/>
    <schema:keywords rdf:resource="https://linked.data.gov.au/def/gswa-vocabulary-themes/resources"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://linked.data.gov.au/def/borehole-start-point/open-pit-floor-or-wall">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://linked.data.gov.au/def/borehole-start-point/open-pit-floor-or-wall</schema:citation>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-start-point"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates that the borehole was drilled from an open pit floor or open pit wall.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">open pit floor or wall</skos:prefLabel>
  </rdf:Description>

  <rdf:Description rdf:about="http://linked.data.gov.au/def/borehole-start-point/underground">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <schema:citation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://linked.data.gov.au/def/borehole-start-point/underground</schema:citation>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-start-point"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates that the borehole was drilled from an underground location.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">underground</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-start-point-setting/manmade-surface">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates that the borehole was drilled from an artificially made or engineered surface, commonly near a natural ground surface. This concept includes drilling originating from tailings and waste dumps, as well as excavations such as quarries and open pit mines.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">man-made surface</skos:prefLabel>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:narrower rdf:resource="http://linked.data.gov.au/def/borehole-start-point/open-pit-floor-or-wall"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting/mining-byproduct-surface"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-start-point-setting/mining-byproduct-surface">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates that the borehole was drilled from artificially made surfaces related to byproducts of exploration and mining operations, such as tailings and waste dumps.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">mining byproduct surface</skos:prefLabel>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-start-point-setting/natural-ground-surface">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates that the borehole was drilled from the natural ground surface or from ground with limited disturbance carried out in preparation for drilling, i.e. shallow excavation works, drill pads, cut and fill.</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">natural ground surface</skos:prefLabel>
    <skos:historyNote>Compiled by GSWA</skos:historyNote>
    <skos:closeMatch rdf:resource="https://linked.data.gov.au/def/borehole-start-point/natural-ground-surface"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-start-point-setting/offshore">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates that the borehole was drilled in marine waters / open sea from a floating vessel or fixed platform, such as a drill ship, jackup or semisubmersible rig, petroleum platform or a marine research vessel, on the seaward side of the baseline of the territorial sea (as defined in the Seas and Submerged Lands Act 1973, Registered 11 September 2019).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">offshore</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:historyNote>This concept is defined in conjunction with &apos;onshore&apos; by taking into account the definition of territorial sea and its baseline as defined in the Seas and Submerged Lands Act 1973, Registered 11 September 2019
https://www.legislation.gov.au/Details/C2008C00402</skos:historyNote>
    <skos:closeMatch rdf:resource="http://linked.data.gov.au/def/borehole-start-point/offshore"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://linked.data.gov.au/def/borehole-start-point-setting/onshore">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">Indicates that the borehole was drilled from or near the natural ground surface. This includes drilling originating on land and on internal waters, i.e. those bodies of water that are located on the landward side of the baseline of the territorial sea (as defined in the Seas and Submerged Lands Act 1973, Registered 11 September 2019).</skos:definition>
    <skos:inScheme rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:prefLabel xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">onshore</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting"/>
    <skos:historyNote>This concept is defined in conjunction with &apos;offshore&apos; by taking into account the definition of territorial sea and its baseline as defined in the Seas and Submerged Lands Act 1973, Registered 11 September 2019
https://www.legislation.gov.au/Details/C2008C00399</skos:historyNote>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting/manmade-surface"/>
    <skos:narrower rdf:resource="https://linked.data.gov.au/def/borehole-start-point-setting/natural-ground-surface"/>
  </rdf:Description>

