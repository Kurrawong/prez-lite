      ],
      "prefLabel": "man-made surface"
    },
    {
      "@id": "https://linked.data.gov.au/def/borehole-start-point-setting/mining-byproduct-surface",
      "@type": "Concept",
      "rdfs:isDefinedBy": {
        "@id": "https://linked.data.gov.au/def/borehole-start-point-setting"
      },
      "definition": "Indicates that the borehole was drilled from artificially made surfaces related to byproducts of exploration and mining operations, such as tailings and waste dumps.",
      "historyNote": "Compiled by GSWA",
      "inScheme": "https://linked.data.gov.au/def/borehole-start-point-setting",
      "prefLabel": "mining byproduct surface"
    },
    {
      "@id": "https://linked.data.gov.au/def/borehole-start-point-setting/natural-ground-surface",
      "@type": "Concept",
      "rdfs:isDefinedBy": {
        "@id": "https://linked.data.gov.au/def/borehole-start-point-setting"
      },
      "closeMatch": {
        "@id": "https://linked.data.gov.au/def/borehole-start-point/natural-ground-surface"
      },
      "definition": "Indicates that the borehole was drilled from the natural ground surface or from ground with limited disturbance carried out in preparation for drilling, i.e. shallow excavation works, drill pads, cut and fill.",
      "historyNote": "Compiled by GSWA",
      "inScheme": "https://linked.data.gov.au/def/borehole-start-point-setting",
      "prefLabel": "natural ground surface"
    },
    {
      "@id": "https://linked.data.gov.au/def/borehole-start-point-setting/offshore",
      "@type": "Concept",
      "rdfs:isDefinedBy": {
        "@id": "https://linked.data.gov.au/def/borehole-start-point-setting"
      },
      "closeMatch": {
        "@id": "http://linked.data.gov.au/def/borehole-start-point/offshore"
      },
      "definition": "Indicates that the borehole was drilled in marine waters / open sea from a floating vessel or fixed platform, such as a drill ship, jackup or semisubmersible rig, petroleum platform or a marine research vessel, on the seaward side of the baseline of the territorial sea (as defined in the Seas and Submerged Lands Act 1973, Registered 11 September 2019).",
      "historyNote": "This concept is defined in conjunction with 'onshore' by taking into account the definition of territorial sea and its baseline as defined in the Seas and Submerged Lands Act 1973, Registered 11 September 2019\nhttps://www.legislation.gov.au/Details/C2008C00402",
      "inScheme": "https://linked.data.gov.au/def/borehole-start-point-setting",
      "prefLabel": "offshore",
      "topConceptOf": "https://linked.data.gov.au/def/borehole-start-point-setting"
    },
    {
      "@id": "https://linked.data.gov.au/def/borehole-start-point-setting/onshore",
      "@type": "Concept",
      "rdfs:isDefinedBy": {
        "@id": "https://linked.data.gov.au/def/borehole-start-point-setting"
      },
      "definition": "Indicates that the borehole was drilled from or near the natural ground surface. This includes drilling originating on land and on internal waters, i.e. those bodies of water that are located on the landward side of the baseline of the territorial sea (as defined in the Seas and Submerged Lands Act 1973, Registered 11 September 2019).",
      "historyNote": "This concept is defined in conjunction with 'offshore' by taking into account the definition of territorial sea and its baseline as defined in the Seas and Submerged Lands Act 1973, Registered 11 September 2019\nhttps://www.legislation.gov.au/Details/C2008C00399",
      "inScheme": "https://linked.data.gov.au/def/borehole-start-point-setting",
